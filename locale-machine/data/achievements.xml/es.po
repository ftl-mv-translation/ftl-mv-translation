#: src-en/data/achievements.xml:121
msgid "data/achievements.xml$/FTL/achievement[18]/name"
msgstr "Objetivo de embarque exitoso"

#: src-en/data/achievements.xml:122
msgid "data/achievements.xml$/FTL/achievement[18]/desc"
msgstr ""
