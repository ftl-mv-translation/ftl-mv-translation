msgstr "Les attaquer."

#: src-en/data/events_sector_coalition.xml:99
msgid ""
"data/events_sector_coalition.xml$//event[@name=\"COALITION_FED_RECON\"]/choice[@req=\"SEARCHING_FOR_CLOAKED_FED\"]/event/choice[@req=\"rep_comb_federation\""
" and @lvl=\"1\"]/event/choice[2]/event/text"
msgstr ""
"\"Bon sang ! Je savais qu'on ne pouvait pas leur faire confiance !\" entend-"
"on dire alors qu'ils se précipitent vers leurs postes de combat."

#: src-en/data/events_sector_coalition.xml:102
msgid ""
"data/events_sector_coalition.xml$//event[@name=\"COALITION_FED_RECON\"]/choice[@req=\"SEARCHING_FOR_CLOAKED_FED\"]/event/choice[@req=\"rep_comb_federation\""
" and @lvl=\"1\"]/event/choice[2]/event/choice/text"
msgstr "Continuer..."

#: src-en/data/events_sector_coalition.xml:110
msgid ""
"data/events_sector_coalition.xml$//event[@name=\"COALITION_FED_RECON\"]/choice[@req=\"SEARCHING_FOR_CLOAKED_FED\"]/event/choice[@req=\"rep_comb_federation\""
