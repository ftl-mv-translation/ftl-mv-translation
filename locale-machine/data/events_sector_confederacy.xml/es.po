msgstr "Continuar..."

#: src-en/data/events_sector_confederacy.xml:2340
msgid ""
"data/events_sector_confederacy.xml$//event[@name=\"IVAR_SAVED\"]/choice/event/choice/event/choice[@req=\"pilot\"]/event/text"
msgstr ""
