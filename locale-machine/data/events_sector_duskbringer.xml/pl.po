"data/events_sector_duskbringer.xml$//event[@name=\"DUSK_SNEAKYREBEL\"]/choice[5]/event/text"
msgstr "Śledzenie Rebeliantów na tak niebezpiecznym terytorium to zły pomysł."

#: src-en/data/events_sector_duskbringer.xml:1129
msgid ""
"data/events_sector_duskbringer.xml$//event[@name=\"DUSK_SNEAKYREBEL\"]/choice[5]/event/choice[1]/text"
msgstr "Zrób coś na statku."

#: src-en/data/events_sector_duskbringer.xml:1133
msgid ""
"data/events_sector_duskbringer.xml$//event[@name=\"DUSK_SNEAKYREBEL\"]/choice[5]/event/choice[2]/text"
