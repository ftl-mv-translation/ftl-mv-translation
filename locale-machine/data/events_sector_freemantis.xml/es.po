msgid ""
"data/events_sector_freemantis.xml$//event[@name=\"FREEMANTIS_DIPLOMACY\"]/choice[4]/text"
msgstr "Mantenerse al margen."

#: src-en/data/events_sector_freemantis.xml:996
msgid ""
"data/events_sector_freemantis.xml$//event[@name=\"FREEMANTIS_DIPLOMACY\"]/choice[4]/event/text"
msgstr ""
