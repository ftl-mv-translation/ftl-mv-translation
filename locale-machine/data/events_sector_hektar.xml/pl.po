msgstr "(Anurak) Anurak wie, jak otworzyć drzwi."

#: src-en/data/events_sector_hektar.xml:1450
msgid ""
"data/events_sector_hektar.xml$//event[@name=\"NEBULA_SYLVANHOME_REAL\"]/choice[1]/event/choice[@req=\"unique_sylvan\"]/text"
msgstr "(Sylvan) Sylvan wydaje się mieć coś do powiedzenia."

#: src-en/data/events_sector_hektar.xml:1452
