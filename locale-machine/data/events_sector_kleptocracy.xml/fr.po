msgid ""
"data/events_sector_kleptocracy.xml$//event[@name=\"DISTRESS_REFUGEE_SLUG_MV\"]/choice[2]/text"
msgstr "Ignorer les réfugiés."

#: src-en/data/events_sector_kleptocracy.xml:122
msgid ""
"data/events_sector_kleptocracy.xml$//event[@name=\"DISTRESS_REFUGEE_SLUG_MV\"]/choice[2]/event/text"
msgstr ""
"Vous vous méfiez des réfugiés mais vous pensez qu'il n'y a pas de danger à "
"faire quelque chose sur le vaisseau."

#: src-en/data/events_sector_kleptocracy.xml:124
msgid ""
"data/events_sector_kleptocracy.xml$//event[@name=\"DISTRESS_REFUGEE_SLUG_MV\"]/choice[2]/event/choice[1]/text"
msgstr "Faire quelque chose à bord du vaisseau."

