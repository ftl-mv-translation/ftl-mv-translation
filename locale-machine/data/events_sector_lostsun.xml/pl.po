msgstr "Główny statek Utraconego Słońca dziękuje ci za pomoc i płaci."

#: src-en/data/events_sector_lostsun.xml:957
msgid ""
"data/events_sector_lostsun.xml$//event[@name=\"QUEST_LOSTSUN_DISRUPTION_QUEST\"]/choice[@req=\"PDS_DISABLE\"]/text"
msgstr "(Rozcieńczacz Sygnału) Uniemożliw stacji wezwanie pomocy."

#: src-en/data/events_sector_lostsun.xml:959
