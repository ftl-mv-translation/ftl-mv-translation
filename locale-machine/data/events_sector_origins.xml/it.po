msgid ""
"data/events_sector_origins.xml$//event[@name=\"CULTIST_CHURCH_QUESTION\"]/choice[2]/event/choice[1]/event/choice/event/choice/event/choice[2]/text"
msgstr "Non possiamo rischiare, correre!"

#: src-en/data/events_sector_origins.xml:1376
msgid ""
"data/events_sector_origins.xml$//event[@name=\"CULTIST_CHURCH_QUESTION\"]/choice[2]/event/choice[1]/event/choice/event/choice/event/choice[2]/event/text"
msgstr ""
"Hai abbastanza problemi da affrontare, come le orde dei cultisti che "
"brulicano di attaccare la tua nave."

