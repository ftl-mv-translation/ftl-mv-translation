#: src-en/data/events_sector_pirate.xml:3414
msgid ""
"data/events_sector_pirate.xml$//event[@name=\"TUCO_SEARCH\"]/choice[1]/event/choice[2]/text"
msgstr "Ayudar a los Ejecutores del Casino."

#: src-en/data/events_sector_pirate.xml:3416
msgid ""
"data/events_sector_pirate.xml$//event[@name=\"TUCO_SEARCH\"]/choice[1]/event/choice[2]/event/text"
msgstr ""
"Estás seguro de que el casino te recompensará si los salvas del ataque de "
"Tuco."

#: src-en/data/events_sector_pirate.xml:3419
msgid ""
