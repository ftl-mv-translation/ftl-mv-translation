#: src-en/data/events_sector_spider.xml:983
msgid ""
"data/events_sector_spider.xml$//event[@name=\"DISTRESS_INFESTATION_CIVILIAN\"]/choice[2]/text"
msgstr "Alejate de eso."

#: src-en/data/events_sector_spider.xml:985
msgid ""
"data/events_sector_spider.xml$//event[@name=\"DISTRESS_INFESTATION_CIVILIAN\"]/choice[2]/event/text"
msgstr ""
"La pelea los saca de su rango de escaneo inmediato.Después de un tiempo, las"
" llamadas de angustia se detienen."

#: src-en/data/events_sector_spider.xml:987
msgid ""
"data/events_sector_spider.xml$//event[@name=\"DISTRESS_INFESTATION_CIVILIAN\"]/choice[2]/event/choice[1]/text"
msgstr "Haz algo a bordo del barco."

#: src-en/data/events_sector_spider.xml:991
