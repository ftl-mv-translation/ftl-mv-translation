msgstr "Continua..."

#: src-en/data/events_special_curator.xml:1054
msgid ""
"data/events_special_curator.xml$//event[@name=\"CURA_PLUSHIE_SUCCESS\"]/choice[@req=\"LIST_CHECK_ANY_CREW\""
" and @lvl=\"0\"]/text"
msgstr "Dovrei andare."

#: src-en/data/events_special_curator.xml:1058
msgid ""
