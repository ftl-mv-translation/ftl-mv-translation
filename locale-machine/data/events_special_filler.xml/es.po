#: src-en/data/events_special_filler.xml:5177
msgid ""
"data/events_special_filler.xml$//event[@name=\"FILLER_DELIVERY\"]/choice[2]/event/choice/text"
msgstr "Continuar..."

#: src-en/data/events_special_filler.xml:5183
msgid ""
"data/events_special_filler.xml$//event[@name=\"FILLER_DELIVERY\"]/choice[3]/text"
