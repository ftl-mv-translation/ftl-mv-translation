#: src-en/data/events_special_filler.xml:2987
msgid ""
"data/events_special_filler.xml$//event[@name=\"FILLER_DISTRESS_REFUGEE\"]/choice[2]/event/text"
msgstr ""
"Tienes cuidado con los refugiados, pero cree que es seguro hacer algo en el "
"barco."

#: src-en/data/events_special_filler.xml:2989
msgid ""
"data/events_special_filler.xml$//event[@name=\"FILLER_DISTRESS_REFUGEE\"]/choice[2]/event/choice[1]/text"
msgstr "Haz algo a bordo del barco."

#: src-en/data/events_special_filler.xml:2993
