"data/events_special_guards.xml$//event[@name=\"GUARD_HUNTGROUNDS\"]/choice[@req=\"LIST_CREW_ZOLTAN\""
" and @lvl=\"1\"]/event/choice[@req=\"unique_anurak\"]/text"
msgstr "Lascia che prendano Anurak e procedi."

#: src-en/data/events_special_guards.xml:8414
msgid ""
"data/events_special_guards.xml$//event[@name=\"GUARD_HUNTGROUNDS\"]/choice[@req=\"LIST_CREW_ZOLTAN\""
" and @lvl=\"1\"]/event/choice[@req=\"unique_anurak\"]/event/text"
