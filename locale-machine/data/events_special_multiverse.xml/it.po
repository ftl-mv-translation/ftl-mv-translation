#: src-en/data/events_special_multiverse.xml:5052
msgid ""
"data/events_special_multiverse.xml$//textList[@name=\"CALLIE_DEAD\"]/text[1]"
msgstr ""
"\"Cattivo [NOME STUDENTE], cattivo [NOME STUDENTE]! I tuoi genitori lo "
"SAPRANNO!\""

#: src-en/data/events_special_multiverse.xml:5053
msgid ""
"data/events_special_multiverse.xml$//textList[@name=\"CALLIE_DEAD\"]/text[2]"
msgstr ""
"\"Che maleducato, [NOME STUDENTE]! Devi imparare un po' di b-b-b-b-buone "
"maniere!\""

#: src-en/data/events_special_multiverse.xml:5054
msgid ""
"data/events_special_multiverse.xml$//textList[@name=\"CALLIE_DEAD\"]/text[3]"
msgstr ""
"\"Ho fallito come tuo insegnante! Forse il mio sostituto sarà in grado di "
"insegnarti meglio! Oh, povera me!\""

#: src-en/data/events_special_multiverse.xml:5056
msgid ""
"data/events_special_multiverse.xml$//textList[@name=\"CALLIE_DEAD\"]/text[4]"
msgstr ""
"\"Va bene [NOME STUDENTE]! Nessun rancore! Ma tanto per fartelo sapere, fai "
"schifo in matematica! Spero tu muoia presto! Ciao ciao!\""

#: src-en/data/events_special_multiverse.xml:5057
msgid ""
"data/events_special_multiverse.xml$//textList[@name=\"CALLIE_DEAD\"]/text[5]"
msgstr ""
"\"Le mie scuse Signor Wentworth, ho fallito! Sfortunatamente, [NOME "
"STUDENTE] è semplicemente troppo stupido per imparare! E ora muoio!\""

