"data/hyperspace.xml$//customShip[@name=\"CREW_SHIP_SLOT3_2\"]/crew/unique_ooj/@name"
msgstr "OOJ MAJOO"

#: src-en/data/hyperspace.xml:4744
msgid ""
"data/hyperspace.xml$//customShip[@name=\"CREW_SHIP_SLOT3_3\"]/crew/unique_ellie_stephan/@name"
msgstr "Stephan"

#: src-en/data/hyperspace.xml:4745
msgid ""
