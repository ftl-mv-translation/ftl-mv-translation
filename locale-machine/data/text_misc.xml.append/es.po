msgstr "2x Daño del casco en habitaciones sin sistema."

#: src-en/data/text_misc.xml.append:221
msgid "data/text_misc.xml.append$//text[@name=\"redesigned_damage_room\"]"
msgstr "Daño normal por habitación: \\1 (x15 para la tripulación)"

#: src-en/data/text_misc.xml.append:222
msgid "data/text_misc.xml.append$//text[@name=\"redesigned_damage_shot\"]"
msgstr "Daño normal por disparo: \\1 (x15 para la tripulación)"

#: src-en/data/text_misc.xml.append:223
