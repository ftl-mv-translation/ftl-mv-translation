#: src-en/data/text_misc.xml.append:220
msgid "data/text_misc.xml.append$//text[@name=\"redesigned_double_damage\"]"
msgstr "2x danos causados pelo casco em salas sem sistema."

#: src-en/data/text_misc.xml.append:221
msgid "data/text_misc.xml.append$//text[@name=\"redesigned_damage_room\"]"
msgstr "Dano normal por quarto: \\1 (x15 para a tripulação)"

#: src-en/data/text_misc.xml.append:222
msgid "data/text_misc.xml.append$//text[@name=\"redesigned_damage_shot\"]"
msgstr "Dano normal por tiro: \\1 (x15 para a tripulação)"

#: src-en/data/text_misc.xml.append:223
msgid "data/text_misc.xml.append$//text[@name=\"hull_damage\"]"
msgstr "Dano de casco base: \\1"

