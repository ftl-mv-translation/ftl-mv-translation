"scafo agli astrofari vuoti per ottenere moduli extra."

#: src-en/data/blueprints.xml.append:11049
msgid "data/blueprints.xml.append$//augBlueprint[@name=\"SCRAP_HULL\"]/title"
msgstr "Cupidigia"

#: src-en/data/blueprints.xml.append:11050
msgid "data/blueprints.xml.append$//augBlueprint[@name=\"SCRAP_HULL\"]/desc"
