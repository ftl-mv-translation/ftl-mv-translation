"data/dlcEvents_anaerobic.xml.append$//event[@name=\"LANIUS_LUNG_SCAN\"]/choice[@req=\"human_medic\"]/text"
msgstr "Дать им просканировать медика."

#: src-en/data/dlcEvents_anaerobic.xml.append:259
msgid ""
"data/dlcEvents_anaerobic.xml.append$//event[@name=\"LANIUS_LUNG_SCAN\"]/choice[@req=\"human_medic\"]/event/text"
msgstr ""
