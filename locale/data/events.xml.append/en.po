msgid ""
"data/events.xml.append$//event[@name=\"START_BEACON_TUTORIAL\"]/eventButton[@name=\"UI_STORAGE_CHECK_BUTTON\"]/inactiveTooltip"
msgstr "Unavailable when the ship is in danger."

#: src-en/data/events.xml.append:115
msgid "data/events.xml.append$//eventButton[@name=\"UI_JUKEBOX\"]/tooltip"
msgstr "Open the jukebox."

#: src-en/data/events.xml.append:116
msgid "data/events.xml.append$//eventButton[@name=\"UI_JUKEBOX\"]/inactiveTooltip"
msgstr "Unavailable."

#: src-en/data/events.xml.append:123
msgid "data/events.xml.append$//eventButton[@name=\"UI_TOGGLE_BUTTON\"]/tooltip"
msgstr "Toggle a drone or weapon."

#: src-en/data/events.xml.append:124
msgid ""
"data/events.xml.append$//eventButton[@name=\"UI_TOGGLE_BUTTON\"]/inactiveTooltip"
msgstr "Unavailable!"

#: src-en/data/events.xml.append:138
msgid ""
"data/events.xml.append$//event[@name=\"START_BEACON_TUTORIAL\"]/choice[1]/text"
msgstr "Go to the tutorial."

#: src-en/data/events.xml.append:142
msgid ""
"data/events.xml.append$//event[@name=\"START_BEACON_TUTORIAL\"]/choice[2]/text"
msgstr "Skip the tutorial."

#: src-en/data/events.xml.append:144
msgid "data/events.xml.append$//event[@name=\"START_BEACON_TUTORIAL_SKIP\"]/text"
msgstr ""
"Are you sure you want to skip the tutorial? This is recommended only those who have already played Multiverse since version 5.0.\n"
"\t\t\t\n"
"\t\t\tYou may also skip the tutorial now and play it again at a later time at the start of any new run."

#: src-en/data/events.xml.append:148
msgid ""
"data/events.xml.append$//event[@name=\"START_BEACON_TUTORIAL_SKIP\"]/choice[1]/text"
msgstr "Nevermind, take me to the tutorial."

#: src-en/data/events.xml.append:152
msgid ""
"data/events.xml.append$//event[@name=\"START_BEACON_TUTORIAL_SKIP\"]/choice[2]/text"
msgstr "Yes, I would like to skip the tutorial."

#: src-en/data/events.xml.append:159
msgid "data/events.xml.append$//event[@name=\"START_BEACON_FAKE\"]/text"
msgstr ""
"You take a look at the distant swirls of space as the stars fly past. This "
"is the beginning of the next chapter in your saga. What will await you next?"

#: src-en/data/events.xml.append:161
msgid "data/events.xml.append$//event[@name=\"START_BEACON_FAKE\"]/choice/text"
msgstr "Continue..."

