msgstr "Fournisser de fausses données."

#: src-en/data/events_ancient.xml:2730
msgid "data/events_ancient.xml$//event[@name=\"ROYAL_DATA\"]/choice[3]/text"
msgstr "Refuser."

#: src-en/data/events_ancient.xml:2732
msgid "data/events_ancient.xml$//event[@name=\"ROYAL_DATA\"]/choice[3]/event/text"
msgstr ""
"\"C'est très malheureux. Maintenant, je suis triste. Au revoir.\" Le "
"vaisseau s'éloigne."

#: src-en/data/events_ancient.xml:2734
msgid ""
"data/events_ancient.xml$//event[@name=\"ROYAL_DATA\"]/choice[3]/event/choice[1]/text"
msgstr "Faire quelque chose à bord du vaisseau."

