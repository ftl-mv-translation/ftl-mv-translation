"data/events_anomaly.xml$//event[@name=\"ANOMALY_QUESTIONS_2\"]/choice[3]/text"
msgstr "我对此表示怀疑。"

#: src-en/data/events_anomaly.xml:181
msgid ""
"data/events_anomaly.xml$//event[@name=\"ANOMALY_QUESTIONS_2\"]/choice[3]/event/text"
msgstr "\"嗯。人们从不相信那个......但我们应该继续。\""

#: src-en/data/events_anomaly.xml:183
msgid ""
"data/events_anomaly.xml$//event[@name=\"ANOMALY_QUESTIONS_2\"]/choice[3]/event/choice/text"
