#: src-en/data/events_boss.xml.append:937
msgid ""
"data/events_boss.xml.append$//event[@name=\"FLAGSHIP_JERRY\"]/choice/event/choice/event/choice/event/choice/event/choice/event/choice[3]/event/choice/event/choice/event/text"
msgstr "Enfin, Jerry monte sur le poney ! \"Voilà. Maintenant, je suis prêt.\""

#: src-en/data/events_boss.xml.append:939
msgid ""
