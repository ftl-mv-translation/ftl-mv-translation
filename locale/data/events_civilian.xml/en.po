#: src-en/data/events_civilian.xml:2655
msgid ""
"data/events_civilian.xml$//event[@name=\"DETECTIVE_TALK_THOM\"]/choice[7]/text"
msgstr "Ask Thom about evidence."

#: src-en/data/events_civilian.xml:2657
msgid ""
"data/events_civilian.xml$//event[@name=\"DETECTIVE_TALK_THOM\"]/choice[7]/event/text"
msgstr ""
"What evidence do you want to ask Thom about?\n"
"\t\t\t[As you investigate more clues or evidence, more options will appear here]"

#: src-en/data/events_civilian.xml:2660
msgid ""
"data/events_civilian.xml$//event[@name=\"DETECTIVE_TALK_THOM\"]/choice[7]/event/choice[1]/text"
msgstr "Ask Thom about the Drone system."

#: src-en/data/events_civilian.xml:2662
msgid ""
"data/events_civilian.xml$//event[@name=\"DETECTIVE_TALK_THOM\"]/choice[7]/event/choice[1]/event/text"
msgstr ""
"\"Nope, never had the head for any of that drone stuff. I trust Doctor "
"Browne for that kind of stuff, because I can't even figure out how to turn "
"the drones on in the first place!\""

#: src-en/data/events_civilian.xml:2664
msgid ""
"data/events_civilian.xml$//event[@name=\"DETECTIVE_TALK_THOM\"]/choice[7]/event/choice[1]/event/choice[1]/text"
msgstr "Call Thom out on his lie."

