#: src-en/data/events_civilian.xml:671
msgid ""
"data/events_civilian.xml$//event[@name=\"COREWORLDS_FREEMANTIS_LOST\"]/choice[1]/event/choice[1]/event/choice[3]/event/choice/event/choice[2]/event/text"
msgstr "The Free Mantis transfer over some fuel."

