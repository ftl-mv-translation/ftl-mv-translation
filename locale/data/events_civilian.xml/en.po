#: src-en/data/events_civilian.xml:1193
msgid ""
"data/events_civilian.xml$//event[@name=\"QUEST_GATLING_BARREL\"]/choice[2]/text"
msgstr "This is risky, we should leave."

#: src-en/data/events_civilian.xml:1195
msgid ""
"data/events_civilian.xml$//event[@name=\"QUEST_GATLING_BARREL\"]/choice[2]/event/text"
msgstr ""
"You decide against pulling closer to the camp and stay far away from the "
"moon."

#: src-en/data/events_civilian.xml:1197
msgid ""
"data/events_civilian.xml$//event[@name=\"QUEST_GATLING_BARREL\"]/choice[2]/event/choice[1]/text"
