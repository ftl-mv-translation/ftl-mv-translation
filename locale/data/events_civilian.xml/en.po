msgid ""
"data/events_civilian.xml$//event[@name=\"DETECTIVE_TALK_THOM\"]/choice[5]/text"
msgstr "Ask Thom about the other suspects."

#: src-en/data/events_civilian.xml:2621
msgid ""
"data/events_civilian.xml$//event[@name=\"DETECTIVE_TALK_THOM\"]/choice[5]/event/text"
msgstr "Which suspect do you want to ask Thom about?"

#: src-en/data/events_civilian.xml:2623
msgid ""
"data/events_civilian.xml$//event[@name=\"DETECTIVE_TALK_THOM\"]/choice[5]/event/choice[1]/text"
msgstr "Amy."

#: src-en/data/events_civilian.xml:2625
msgid ""
"data/events_civilian.xml$//event[@name=\"DETECTIVE_TALK_THOM\"]/choice[5]/event/choice[1]/event/text"
msgstr ""
"\"Amy? She's uh, she's something I guess. She's always yapping at us, and "
"she has the shortest temper I've ever seen. She does a good job though, and "
"I wouldn't ever think she'd be capable of murder! Though, she never did like"
" Henry.\""

#: src-en/data/events_civilian.xml:2627
msgid ""
"data/events_civilian.xml$//event[@name=\"DETECTIVE_TALK_THOM\"]/choice[5]/event/choice[1]/event/choice/text"
msgstr "Continue..."

#: src-en/data/events_civilian.xml:2633
msgid ""
"data/events_civilian.xml$//event[@name=\"DETECTIVE_TALK_THOM\"]/choice[5]/event/choice[2]/text"
msgstr "Daniel."

#: src-en/data/events_civilian.xml:2635
msgid ""
"data/events_civilian.xml$//event[@name=\"DETECTIVE_TALK_THOM\"]/choice[5]/event/choice[2]/event/text"
msgstr ""
"\"Dr. Browne? Oh he's such a kind old man, and dedicated to his work too. I "
"can imagine he's quite shocked about the murder, he was always so close to "
"poor Henry...\""

#: src-en/data/events_civilian.xml:2637
msgid ""
"data/events_civilian.xml$//event[@name=\"DETECTIVE_TALK_THOM\"]/choice[5]/event/choice[2]/event/choice/text"
msgstr "Continue..."

#: src-en/data/events_civilian.xml:2645
msgid ""
"data/events_civilian.xml$//event[@name=\"DETECTIVE_TALK_THOM\"]/choice[6]/text"
msgstr "Spit in Thom's face."

#: src-en/data/events_civilian.xml:2647
msgid ""
"data/events_civilian.xml$//event[@name=\"DETECTIVE_TALK_THOM\"]/choice[6]/event/text"
msgstr ""
"For no reason in particular, you decide to spit at Thom. Sherlock glares at "
"you but says nothing."

#: src-en/data/events_civilian.xml:2649
msgid ""
"data/events_civilian.xml$//event[@name=\"DETECTIVE_TALK_THOM\"]/choice[6]/event/choice/text"
