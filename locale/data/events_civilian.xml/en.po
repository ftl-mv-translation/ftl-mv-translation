#: src-en/data/events_civilian.xml:1270
msgid ""
"data/events_civilian.xml$//event[@name=\"QUEST_GATLING_BASE\"]/choice[1]/event/choice[4]/event/choice/text"
msgstr "Continue..."

#: src-en/data/events_civilian.xml:1278
msgid ""
"data/events_civilian.xml$//event[@name=\"QUEST_GATLING_BASE\"]/choice[2]/text"
msgstr "We have better things to do than dishonouring the dead."

#: src-en/data/events_civilian.xml:1280
