msgid ""
"data/events_coalition.xml$//event[@name=\"COALITION_HEKTAR\"]/choice[@req=\"LIST_VARIANT_HEKTAR\"]/text"
msgstr "(赫克塔船员)申请员工折扣。"

#: src-en/data/events_coalition.xml:114
