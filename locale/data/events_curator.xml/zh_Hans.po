msgid ""
"data/events_curator.xml$//event[@name=\"CURA_SURVIVOR\"]/choice[1]/event/text"
msgstr "\"呸，变元者以为他们是在挑战塔基佩特？你是愚蠢的存在！准备去死吧！\""

#: src-en/data/events_curator.xml:1339
