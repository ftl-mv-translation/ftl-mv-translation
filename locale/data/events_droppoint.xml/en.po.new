#: src-en/data/events_droppoint.xml:838
msgid ""
"data/events_droppoint.xml$//event[@name=\"MV_ASTEROID_EXPLORE\"]/choice[@req=\"BEAM_MINING\"]/text"
msgstr "(Mining Equipment) Attempt to mine the asteroids."

#: src-en/data/events_droppoint.xml:840
msgid ""
"data/events_droppoint.xml$//event[@name=\"MV_ASTEROID_EXPLORE\"]/choice[@req=\"BEAM_MINING\"]/event/text"
