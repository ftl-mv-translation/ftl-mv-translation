msgid ""
"data/events_estate.xml$//event[@name=\"ESTATE_INVESTOR_QUESTIONS_MET\"]/choice[3]/text"
msgstr "Eh. I've seen worse."

#: src-en/data/events_estate.xml:832
msgid ""
