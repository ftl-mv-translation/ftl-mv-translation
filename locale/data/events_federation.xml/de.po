#: src-en/data/events_federation.xml:533
msgid ""
"data/events_federation.xml$//event[@name=\"FED_HANGAR_RAID\"]/choice[2]/event/text"
msgstr "Es ist verlockend, aber du solltest keine Zeit verschwenden."

#: src-en/data/events_federation.xml:535
msgid ""
"data/events_federation.xml$//event[@name=\"FED_HANGAR_RAID\"]/choice[2]/event/choice[1]/text"
msgstr "Tue etwas an Bord des Schiffs."

