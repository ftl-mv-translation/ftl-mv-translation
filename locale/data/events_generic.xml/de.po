#: src-en/data/events_generic.xml:583
msgid ""
"data/events_generic.xml$//event[@name=\"ASTEROID_ENGILAB\"]/choice[2]/event/text"
msgstr ""
"\"Kritischer Fehler. Unerwartet Feindsehligkeit von Föderationsschiff. "
"Richtige Antwort: ‘Oh Scheiße‘. Aktiviere Kampfprotokoll.\""

#: src-en/data/events_generic.xml:587
msgid ""
"data/events_generic.xml$//event[@name=\"ASTEROID_ENGILAB\"]/choice[2]/event/choice/text"
msgstr "Fortfahren..."

#: src-en/data/events_generic.xml:593
msgid "data/events_generic.xml$//event[@name=\"ASTEROID_ENGILAB\"]/choice[3]/text"
msgstr "Wir sind keine Auftragserlediger."

#: src-en/data/events_generic.xml:595
msgid ""
"data/events_generic.xml$//event[@name=\"ASTEROID_ENGILAB\"]/choice[3]/event/text"
msgstr ""
"\"Deine Antwort wurde ausgewertet als: Gemein. Unvorteilhafte Umstände. Wir "
"werden Kontakt mit deinem Schiff beenden.\""

#: src-en/data/events_generic.xml:597
msgid ""
"data/events_generic.xml$//event[@name=\"ASTEROID_ENGILAB\"]/choice[3]/event/choice[1]/text"
msgstr "Tue etwas an Bord des Schiffs."

