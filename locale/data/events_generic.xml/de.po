#: src-en/data/events_generic.xml:727
msgid ""
"data/events_generic.xml$//event[@name=\"DISTRESS_ASTEROID_BACTERIA\"]/choice[2]/event/text"
msgstr "\"Gah, du bist wertlos. Warum ist keiner bereit FÜR mich zu arbeiten?!\""

#: src-en/data/events_generic.xml:729
