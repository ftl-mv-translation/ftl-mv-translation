#: src-en/data/events_guards.xml:10102
msgid ""
"data/events_guards.xml$//event[@name=\"GUARD_LANIUS_DEADCREW\"]/choice/text"
msgstr "Continue..."

#: src-en/data/events_guards.xml:10118
msgid "data/events_guards.xml$//event[@name=\"GUARD_LANIUS_FOLLOWUP\"]/text"
msgstr ""
"By killing the Lanius guard, you have no doubt caused them to dispatch "
"reinforcements to track you down. Watch your step as you explore the sector."

#: src-en/data/events_guards.xml:10121
msgid ""
"data/events_guards.xml$//event[@name=\"GUARD_LANIUS_FOLLOWUP\"]/choice[1]/text"
msgstr "Do something onboard the ship."

#: src-en/data/events_guards.xml:10125
msgid ""
"data/events_guards.xml$//event[@name=\"GUARD_LANIUS_FOLLOWUP\"]/choice[2]/text"
msgstr "Do nothing."

#: src-en/data/events_guards.xml:10133
msgid ""
"data/events_guards.xml$//event[@name=\"REINFORCEMENTS_LANIUS\"]/beaconType/unvisitedTooltip"
msgstr "An unvisited location."

#: src-en/data/events_guards.xml:10134
msgid ""
"data/events_guards.xml$//event[@name=\"REINFORCEMENTS_LANIUS\"]/beaconType/visitedTooltip"
