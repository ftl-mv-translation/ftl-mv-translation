#: src-en/data/events_guards.xml:10361
msgid "data/events_guards.xml$//event[@name=\"GUARD_MONK\"]/choice[1]/text"
msgstr "Ignore the ship."

#: src-en/data/events_guards.xml:10363
msgid "data/events_guards.xml$//event[@name=\"GUARD_MONK\"]/choice[1]/event/text"
msgstr "You move past the auto-ship."

#: src-en/data/events_guards.xml:10365
