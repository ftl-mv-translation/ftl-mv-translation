msgid ""
"data/events_guards.xml$//eventList[@name=\"GUARD_CRYSTAL_DEMAND\"]/event[1]/text"
msgstr ""
"\"You insult our entire species with your demands! We refuse to allow "
"someone such as yourself continue into our sector and wreck havoc!\""

#: src-en/data/events_guards.xml:1776
msgid ""
"data/events_guards.xml$//eventList[@name=\"GUARD_CRYSTAL_DEMAND\"]/event[1]/choice/text"
msgstr "Continue..."

