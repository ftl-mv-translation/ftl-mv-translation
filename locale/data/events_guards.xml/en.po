msgid ""
"data/events_guards.xml$//event[@name=\"GUARD_DYNASTY\"]/choice[@req=\"mind\"]/text"
msgstr "(Mind Control) Convince the guard to leave you alone."

#: src-en/data/events_guards.xml:5096
msgid ""
"data/events_guards.xml$//event[@name=\"GUARD_DYNASTY\"]/choice[@req=\"cloaking\"]/text"
msgstr "(Cloaking) Cloak and try to escape."

#: src-en/data/events_guards.xml:5100
msgid ""
"data/events_guards.xml$//event[@name=\"GUARD_DYNASTY\"]/choice[@req=\"engines\"]/text"
msgstr "(Adv. Engines) Try to escape the guard."

#: src-en/data/events_guards.xml:5104
