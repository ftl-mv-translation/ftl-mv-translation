msgstr "Do nothing."

#: src-en/data/events_guards.xml:1956
msgid "data/events_guards.xml$//event[@name=\"GUARD_ORCHID\"]/choice[2]/text"
msgstr "Contact the guard."

#: src-en/data/events_guards.xml:1960
msgid "data/events_guards.xml$//event[@name=\"GUARD_ORCHID\"]/choice[3]/text"
