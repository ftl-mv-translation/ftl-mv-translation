#: src-en/data/events_guards.xml:4489
msgid "data/events_guards.xml$//event[@name=\"GUARD_SLUG\"]/choice[2]/text"
msgstr "Pay the fee."

#: src-en/data/events_guards.xml:4491
msgid "data/events_guards.xml$//event[@name=\"GUARD_SLUG\"]/choice[2]/event/text"
msgstr "\"Ssssmart choiccce!\" The Slug Captain ends the call."

#: src-en/data/events_guards.xml:4498
msgid ""
"data/events_guards.xml$//event[@name=\"GUARD_SLUG\"]/choice[@req=\"slug_knight\"]/text"
msgstr "(Knight Crew) \"Hmmmm... no.\""

