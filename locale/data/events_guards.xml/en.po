msgid ""
"data/events_guards.xml$//event[@name=\"GUARD_FREEMANTIS\"]/choice[1]/event/choice[2]/text"
msgstr "Do nothing."

#: src-en/data/events_guards.xml:1451
msgid "data/events_guards.xml$//event[@name=\"GUARD_FREEMANTIS\"]/choice[2]/text"
msgstr "Contact the guard."

