msgid ""
"data/events_nexus.xml$//event[@name=\"SYLVAN_MAID\"]/choice[@req=\"slug_saboteur\"]/text"
msgstr "(Саботажник) Взломать интернет."

#: src-en/data/events_nexus.xml:2714
msgid "data/events_nexus.xml$//event[@name=\"SYLVAN_MAID\"]/choice[3]/text"
msgstr "Избавить его от страданий."

