#: src-en/data/events_nexus.xml:159
msgid ""
"data/events_nexus.xml$//event[@name=\"NEXUS_PORTAL_2\"]/choice[1]/event/choice[2]/event/text"
msgstr ""
"\"Разззумный выбор, ссстранник. В любом ссслучае, есссть дела поважнее...\""

#: src-en/data/events_nexus.xml:165
msgid "data/events_nexus.xml$//event[@name=\"NEXUS_PORTAL_2\"]/choice[2]/text"
