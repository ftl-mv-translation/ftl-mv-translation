#: src-en/data/events_piracy.xml:216
msgid ""
"data/events_piracy.xml$//event[@name=\"SLAVER_OFFERS\"]/choice[4]/event/choice[1]/text"
msgstr "Заняться чем-либо на корабле."

#: src-en/data/events_piracy.xml:220
