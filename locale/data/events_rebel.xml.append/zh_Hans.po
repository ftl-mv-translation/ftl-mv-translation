#: src-en/data/events_rebel.xml.append:3301
msgid ""
"data/events_rebel.xml.append$//event[@name=\"JERRY_HANGOUT\"]/choice[1]/event/choice/event/choice[1]/event/choice[1]/event/choice[1]/event/choice[1]/text"
msgstr "再来一杯。"

#: src-en/data/events_rebel.xml.append:3303
