#: src-en/data/events_rock.xml.append:617
msgid ""
"data/events_rock.xml.append$//event[@name=\"ROCK_WEAPON_MEASURING\"]/choice[@req=\"artillery\"]/event/choice[@req=\"artillery\"]/event/choice[@req=\"artillery\"]/text"
msgstr "(Lvl 3 Artillery) How about this!"

#: src-en/data/events_rock.xml.append:619
msgid ""
"data/events_rock.xml.append$//event[@name=\"ROCK_WEAPON_MEASURING\"]/choice[@req=\"artillery\"]/event/choice[@req=\"artillery\"]/event/choice[@req=\"artillery\"]/event/text"
