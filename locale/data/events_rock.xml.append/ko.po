msgid ""
"data/events_rock.xml.append$//event[@name=\"OUTCAST_STORIES\"]/choice[1]/event/choice[1]/event/choice[1]/text"
msgstr "투코의 첫 번째 이야기, 바크로스 4계의 항전."

