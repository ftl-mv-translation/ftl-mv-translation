#: src-en/data/events_ruins.xml:91
msgid ""
"data/events_ruins.xml$//event[@name=\"RUINS_SALVAGE\"]/choice[@req=\"unique_anointed\"]/event/text"
msgstr ""
"\"Will trying my best. Words of forefathers rusty in mind, but gist I am "
"getting right. Stand back.\""

#: src-en/data/events_ruins.xml:93
msgid ""
"data/events_ruins.xml$//event[@name=\"RUINS_SALVAGE\"]/choice[@req=\"unique_anointed\"]/event/choice/text"
