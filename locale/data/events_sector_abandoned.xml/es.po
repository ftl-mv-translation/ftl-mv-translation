#: src-en/data/events_sector_abandoned.xml:187
msgid ""
"data/events_sector_abandoned.xml$//event[@name=\"ABANDONED_COLONY\"]/choice[@req=\"PDS_DISABLE\"]/text"
msgstr "(Signal Diluter) Asegúrate de que las torretas defensivas estén fuera de línea."

#: src-en/data/events_sector_abandoned.xml:191
msgid ""
"data/events_sector_abandoned.xml$//event[@name=\"ABANDONED_COLONY\"]/choice[@req=\"FLARE_DISTRACTION\"]/text"
msgstr "(Flares) Lanzá algunas bengalas para distraer las torretas defensivas mientras explorás."

#: src-en/data/events_sector_abandoned.xml:195
msgid ""
"data/events_sector_abandoned.xml$//event[@name=\"ABANDONED_COLONY\"]/choice[@req=\"sensors\"]/text"
