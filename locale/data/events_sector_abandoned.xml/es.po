#: src-en/data/events_sector_abandoned.xml:122
msgid ""
"data/events_sector_abandoned.xml$//event[@name=\"ABANDONED_SLAVER_MOON\"]/choice[1]/event/text"
msgstr "Parece que un traficante de esclavos está vendiendo sus “mercancías” en la luna cercana, sabiendo que está lejos del ojo de la Federación en este sector."

#: src-en/data/events_sector_abandoned.xml:125
msgid ""
"data/events_sector_abandoned.xml$//event[@name=\"ABANDONED_SLAVER_MOON\"]/choice[1]/event/choice[1]/text"
msgstr "Revisá a los esclavos que están ofreciendo."

