#: src-en/data/events_sector_abandoned.xml:115
msgid ""
"data/events_sector_abandoned.xml$//event[@name=\"ABANDONED_SLAVER_MOON\"]/choice[1]/event/text"
msgstr "Il semblerait qu'un esclavagiste vende ses 'biens' sur la lune, loin des yeux de la Fédération et de ses secteurs."

#: src-en/data/events_sector_abandoned.xml:118
msgid ""
"data/events_sector_abandoned.xml$//event[@name=\"ABANDONED_SLAVER_MOON\"]/choice[1]/event/choice[1]/text"
msgstr "Jeter un œil aux esclaves qu'ils proposent."

#: src-en/data/events_sector_abandoned.xml:120
