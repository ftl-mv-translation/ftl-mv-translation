msgid ""
"data/events_sector_abandoned.xml$//event[@name=\"ABANDONED_LANIUS_AUTO_REBEL\"]/choice[1]/event/text"
msgstr "Vous activez vos armes, ce qui attire rapidement l'attention du Lanius."

#: src-en/data/events_sector_abandoned.xml:607
msgid ""
