#: src-en/data/events_sector_abandoned.xml:331
msgid ""
"data/events_sector_abandoned.xml$//event[@name=\"ABANDONED_CIVILIAN_LOST_ITEMS\"]/choice[2]/event/text"
msgstr "I civili sono delusi, ma ti capiscono."

#: src-en/data/events_sector_abandoned.xml:333
msgid ""
"data/events_sector_abandoned.xml$//event[@name=\"ABANDONED_CIVILIAN_LOST_ITEMS\"]/choice[2]/event/choice[1]/text"
