#: src-en/data/events_sector_abandoned.xml:151
msgid ""
"data/events_sector_abandoned.xml$//event[@name=\"ABANDONED_SLAVER_MOON\"]/choice[1]/event/choice[3]/text"
msgstr "Уйти."

#: src-en/data/events_sector_abandoned.xml:157
msgid ""
"data/events_sector_abandoned.xml$//event[@name=\"ABANDONED_SLAVER_MOON\"]/choice[2]/text"
msgstr "Не обращать внимание на луну и сигнал."

#: src-en/data/events_sector_abandoned.xml:159
msgid ""
"data/events_sector_abandoned.xml$//event[@name=\"ABANDONED_SLAVER_MOON\"]/choice[2]/event/text"
msgstr "Очевидно, что это что-то вроде работорговли, и вас это не интересует."

#: src-en/data/events_sector_abandoned.xml:161
msgid ""
"data/events_sector_abandoned.xml$//event[@name=\"ABANDONED_SLAVER_MOON\"]/choice[2]/event/choice[1]/text"
msgstr "Заняться чем-либо на корабле."

#: src-en/data/events_sector_abandoned.xml:165
