#: src-en/data/events_sector_asteroid.xml:618
msgid ""
"data/events_sector_asteroid.xml$//event[@name=\"ASTEROID_ENGILAB\"]/choice[1]/event/choice[2]/event/choice/text"
msgstr "Continue..."

#: src-en/data/events_sector_asteroid.xml:624
msgid ""
"data/events_sector_asteroid.xml$//event[@name=\"ASTEROID_ENGILAB\"]/choice[1]/event/choice[3]/text"
msgstr "Take on the ship populated by Wraiths."

#: src-en/data/events_sector_asteroid.xml:626
msgid ""
"data/events_sector_asteroid.xml$//event[@name=\"ASTEROID_ENGILAB\"]/choice[1]/event/choice[3]/event/text"
