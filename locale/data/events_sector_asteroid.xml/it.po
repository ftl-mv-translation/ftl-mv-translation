#: src-en/data/events_sector_asteroid.xml:56
msgid "data/events_sector_asteroid.xml$//event[@name=\"ASTEROID_REFINERY\"]/text"
msgstr "Una piccola raffineria dell'Unione opera lontano da tutti gli asteroidi. Anche se di solito raffinano solo materie prime, forse puoi pagarli perché raffinino alcuni dei tuoi moduli."

#: src-en/data/events_sector_asteroid.xml:59
msgid ""
"data/events_sector_asteroid.xml$//event[@name=\"ASTEROID_REFINERY\"]/choice[1]/text"
msgstr "Pagali per creare un'arma."

#: src-en/data/events_sector_asteroid.xml:61
msgid ""
"data/events_sector_asteroid.xml$//event[@name=\"ASTEROID_REFINERY\"]/choice[1]/event/text"
msgstr "\"Richiesta un po' fuori dal comune, ma apprezziamo la donazione. Vedremo cosa possiamo fare.\""

#: src-en/data/events_sector_asteroid.xml:69
