#: src-en/data/events_sector_asteroid.xml:125
msgid ""
"data/events_sector_asteroid.xml$//event[@name=\"ASTEROID_CACHE\"]/choice[5]/text"
msgstr "Stiamo già venendo bombardati. Meglio andarsene."

#: src-en/data/events_sector_asteroid.xml:127
msgid ""
"data/events_sector_asteroid.xml$//event[@name=\"ASTEROID_CACHE\"]/choice[5]/event/text"
msgstr "È troppo rischioso cercare di prendere le scorte. Ti allontani dal campo di asteroidi."

#: src-en/data/events_sector_asteroid.xml:129
msgid ""
"data/events_sector_asteroid.xml$//event[@name=\"ASTEROID_CACHE\"]/choice[5]/event/choice[1]/text"
