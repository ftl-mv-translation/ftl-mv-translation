#: src-en/data/events_sector_augmented.xml:1202
msgid ""
"data/events_sector_augmented.xml$//event[@name=\"DISTRESS_AUGMENTED_NEAR_DEATH\"]/text"
msgstr "“¡Ayuda! ¡Ayuda! ¡Vida cayendo! ¡Casco crítico! ¡Destrucción inminente!” Los Augmented están traduciendo frenéticamente su SOS tan rápido como pueden."

#: src-en/data/events_sector_augmented.xml:1208
msgid ""
"data/events_sector_augmented.xml$//event[@name=\"DISTRESS_AUGMENTED_NEAR_DEATH\"]/choice[@req=\"BOMB_HEAL\"]/text"
msgstr "(Bomba Médica) Sana a los Augmented heridos. [-1}]"

