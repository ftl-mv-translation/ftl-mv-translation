#: src-en/data/events_sector_augmented.xml:147
msgid ""
"data/events_sector_augmented.xml$//event[@name=\"ANOINTED_SURRENDER_ACCEPT\"]/text"
msgstr "Aceptas la rendición del Ungido, y ella transfiere una buena cantidad de chatarra. Sin embargo, mientras te preparas para irte, te pide hablar contigo sobre una cosa más."

#: src-en/data/events_sector_augmented.xml:153
msgid ""
"data/events_sector_augmented.xml$//event[@name=\"ANOINTED_SURRENDER_ACCEPT\"]/choice/text"
