#: src-en/data/events_sector_augmented.xml:730
msgid ""
"data/events_sector_augmented.xml$//event[@name=\"AUGMENTED_CHOICE\"]/choice[1]/event/text"
msgstr "Вы включаете свое оружие и сражаетесь с кораблем Возвышенных."

#: src-en/data/events_sector_augmented.xml:733
msgid ""
"data/events_sector_augmented.xml$//event[@name=\"AUGMENTED_CHOICE\"]/choice[1]/event/choice/text"
msgstr "Продолжить..."

#: src-en/data/events_sector_augmented.xml:739
msgid ""
"data/events_sector_augmented.xml$//event[@name=\"AUGMENTED_CHOICE\"]/choice[2]/text"
msgstr "Избегать их."

#: src-en/data/events_sector_augmented.xml:741
