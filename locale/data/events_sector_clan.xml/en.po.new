msgid ""
"data/events_sector_clan.xml$//event[@name=\"NEBULA_DISTRESS_CLAN_GHOST\"]/choice[2]/text"
msgstr "Decline."

#: src-en/data/events_sector_clan.xml:1877
msgid ""
"data/events_sector_clan.xml$//event[@name=\"NEBULA_DISTRESS_CLAN_GHOST\"]/choice[2]/event/text"
msgstr ""
