#: src-en/data/events_sector_clan.xml:1463
msgid ""
"data/events_sector_clan.xml$//event[@name=\"NEBULA_CLAN_SHELL_RADIANT\"]/choice[1]/event/choice[2]/event/text"
msgstr "“¿Quieres ayudar? Bueno, supongo que no viene mal. Podríamos usar algunos suministros si tienes.”"

#: src-en/data/events_sector_clan.xml:1465
msgid ""
"data/events_sector_clan.xml$//event[@name=\"NEBULA_CLAN_SHELL_RADIANT\"]/choice[1]/event/choice[2]/event/choice[1]/text"
msgstr "Darles 7 misiles."

#: src-en/data/events_sector_clan.xml:1467
