#: src-en/data/events_sector_clan.xml:2849
msgid ""
"data/events_sector_clan.xml$//event[@name=\"SALT_QUEST_2_MOTTO_WRONG\"]/text"
msgstr ""
"\"Cccc'est plutôt bon. Mais cccc'est aussssi faux. Tu mens comme un "
"arracheur de dents, cccc'est clair comme le jour.\""

#: src-en/data/events_sector_clan.xml:2851
msgid ""
"data/events_sector_clan.xml$//event[@name=\"SALT_QUEST_2_MOTTO_WRONG\"]/choice/text"
