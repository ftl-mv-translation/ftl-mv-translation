#: src-en/data/events_sector_clan.xml:2531
msgid ""
"data/events_sector_clan.xml$//event[@name=\"SYLVAN_SALT_QUEST_ACCEPT\"]/choice[2]/event/text"
msgstr "\"Aaah, oh pazienza ssstraniero. Magari un'altra volta allora? Sssono convinto che ci rivedremo in futuro!\""

#: src-en/data/events_sector_clan.xml:2536
msgid ""
"data/events_sector_clan.xml$//event[@name=\"SYLVAN_SALT_QUEST_DECLINE\"]/text"
msgstr "\"Ah... sssì colpa mia... non dovrei messscolare affari e quessstioni persssonali, giusssto....\" Sylvan si rabbuia un po', ma mantiene il suo tono allegro. \"Beh, allora che ne dici di dare comunque un'occhiata alla mia merce, ssstraniero?\""

#: src-en/data/events_sector_clan.xml:2538
msgid ""
"data/events_sector_clan.xml$//event[@name=\"SYLVAN_SALT_QUEST_DECLINE\"]/choice[1]/text"
msgstr "Accetta."

#: src-en/data/events_sector_clan.xml:2542
msgid ""
"data/events_sector_clan.xml$//event[@name=\"SYLVAN_SALT_QUEST_DECLINE\"]/choice[2]/text"
