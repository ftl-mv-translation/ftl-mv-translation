#: src-en/data/events_sector_coalition.xml:808
msgid ""
"data/events_sector_coalition.xml$//event[@name=\"COALITION_REBEL_STRANDED\"]/choice[1]/event/choice[1]/event/choice[6]/text"
msgstr "Booby trap the cargo."

#: src-en/data/events_sector_coalition.xml:810
msgid ""
"data/events_sector_coalition.xml$//event[@name=\"COALITION_REBEL_STRANDED\"]/choice[1]/event/choice[1]/event/choice[6]/event/text"
msgstr ""
"You pass over the \"scrap\" to the Rebels. They pry open the crate to see a "
"ticking bomb that lights their ship ablaze. The captain rushes back to the "
"bridge, coughing. \"Ok, you win. Take this garbage and leave us alone.\""

#: src-en/data/events_sector_coalition.xml:815
msgid ""
"data/events_sector_coalition.xml$//event[@name=\"COALITION_REBEL_STRANDED\"]/choice[1]/event/choice[1]/event/choice[6]/event/choice/text"
msgstr "Accept their gracious offer."

#: src-en/data/events_sector_coalition.xml:817
msgid ""
"data/events_sector_coalition.xml$//event[@name=\"COALITION_REBEL_STRANDED\"]/choice[1]/event/choice[1]/event/choice[6]/event/choice/event/text"
msgstr "You take the supplies from them and prepare to leave."

#: src-en/data/events_sector_coalition.xml:824
msgid ""
"data/events_sector_coalition.xml$//event[@name=\"COALITION_REBEL_STRANDED\"]/choice[1]/event/choice[1]/event/choice[7]/text"
msgstr "We can't help."

#: src-en/data/events_sector_coalition.xml:826
msgid ""
"data/events_sector_coalition.xml$//event[@name=\"COALITION_REBEL_STRANDED\"]/choice[1]/event/choice[1]/event/choice[7]/event/text"
msgstr "\"Yeah, not surprised. Get lost.\""

#: src-en/data/events_sector_coalition.xml:832
msgid ""
"data/events_sector_coalition.xml$//event[@name=\"COALITION_REBEL_STRANDED\"]/choice[1]/event/choice[2]/text"
