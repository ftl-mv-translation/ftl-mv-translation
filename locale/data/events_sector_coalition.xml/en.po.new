#: src-en/data/events_sector_coalition.xml:3041
msgid ""
"data/events_sector_coalition.xml$//event[@name=\"NEBULA_COALITIONHOLD_CEALAFORMER_2\"]/choice[6]/text"
msgstr "Move on."

#: src-en/data/events_sector_coalition.xml:3043
msgid ""
"data/events_sector_coalition.xml$//event[@name=\"NEBULA_COALITIONHOLD_CEALAFORMER_2\"]/choice[6]/event/text"
msgstr ""
"You're sure there was something significant about this encounter, but it's "
"too late now."

#: src-en/data/events_sector_coalition.xml:3045
msgid ""
"data/events_sector_coalition.xml$//event[@name=\"NEBULA_COALITIONHOLD_CEALAFORMER_2\"]/choice[6]/event/choice[1]/text"
msgstr "Do something onboard the ship."

#: src-en/data/events_sector_coalition.xml:3049
msgid ""
"data/events_sector_coalition.xml$//event[@name=\"NEBULA_COALITIONHOLD_CEALAFORMER_2\"]/choice[6]/event/choice[2]/text"
msgstr "Do nothing."

