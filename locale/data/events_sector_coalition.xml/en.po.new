msgid ""
"data/events_sector_coalition.xml$//event[@name=\"COALITION_FREEMANTIS_SLAVES\"]/choice[5]/text"
msgstr "We aren't interested in slaves."

#: src-en/data/events_sector_coalition.xml:904
msgid ""
"data/events_sector_coalition.xml$//event[@name=\"COALITION_FREEMANTIS_SLAVES\"]/choice[5]/event/text"
msgstr ""
"\"Oh! Federation so kind, so forgiving. You are true hero!\" You thank the "
"Free Mantis and leave."

#: src-en/data/events_sector_coalition.xml:906
msgid ""
"data/events_sector_coalition.xml$//event[@name=\"COALITION_FREEMANTIS_SLAVES\"]/choice[5]/event/choice[1]/text"
