msgid ""
"data/events_sector_confederacy.xml$//event[@name=\"VAMPWEED_RIOT\"]/choice[2]/text"
msgstr "Avoid the riot."

#: src-en/data/events_sector_confederacy.xml:622
msgid ""
"data/events_sector_confederacy.xml$//event[@name=\"VAMPWEED_RIOT\"]/choice[2]/event/text"
msgstr ""
