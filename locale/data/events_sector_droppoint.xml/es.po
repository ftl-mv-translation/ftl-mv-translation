#: src-en/data/events_sector_droppoint.xml:1994
msgid ""
"data/events_sector_droppoint.xml$//event[@name=\"DISTRESS_DROPPOINT_ZOLTAN\"]/choice[@req=\"battery\"]/text"
msgstr "(Batería de Reserva) Usar tu subsistema de batería para iniciarlo."

#: src-en/data/events_sector_droppoint.xml:1996
msgid ""
"data/events_sector_droppoint.xml$//event[@name=\"DISTRESS_DROPPOINT_ZOLTAN\"]/choice[@req=\"battery\"]/event/text"
msgstr ""
