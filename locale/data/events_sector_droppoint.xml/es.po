#: src-en/data/events_sector_droppoint.xml:1934
msgid ""
"data/events_sector_droppoint.xml$//event[@name=\"DROPPOINT_BULKY_SMUGGLER\"]/choice[3]/event/text"
msgstr ""
"Le das al traficante algo de espacio. No vale la pena la molestia para "
"involucrarse."

#: src-en/data/events_sector_droppoint.xml:1936
msgid ""
"data/events_sector_droppoint.xml$//event[@name=\"DROPPOINT_BULKY_SMUGGLER\"]/choice[3]/event/choice[1]/text"
msgstr "Hacer algo abordo de la nave."

