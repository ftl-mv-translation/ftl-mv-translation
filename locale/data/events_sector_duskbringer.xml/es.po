msgstr "“Aquí, toma esto como agradecimiento. Íbamos a venderlo para comprar más combustible de todos modos.”"

#: src-en/data/events_sector_duskbringer.xml:1682
msgid ""
"data/events_sector_duskbringer.xml$//event[@name=\"DISTRESS_DUSK_FEDERATION\"]/choice[@req=\"BLUELIST_BOMB_REPAIR\"]/text"
msgstr "(Cluster de Reparación) Usar la bomba para reparar sus motores."

#: src-en/data/events_sector_duskbringer.xml:1684
msgid ""
"data/events_sector_duskbringer.xml$//event[@name=\"DISTRESS_DUSK_FEDERATION\"]/choice[@req=\"BLUELIST_BOMB_REPAIR\"]/event/text"
msgstr "La bomba funciona a la perfección, sellando la brecha."

#: src-en/data/events_sector_duskbringer.xml:1689
msgid ""
"data/events_sector_duskbringer.xml$//event[@name=\"DISTRESS_DUSK_FEDERATION\"]/choice[@req=\"BLUELIST_BOMB_REPAIR\"]/event/choice/text"
msgstr "Continuar..."

#: src-en/data/events_sector_duskbringer.xml:1691
msgid ""
