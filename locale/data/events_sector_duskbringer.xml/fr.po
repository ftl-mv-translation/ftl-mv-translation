msgid ""
"data/events_sector_duskbringer.xml$//event[@name=\"DUSK_IDOL\"]/choice/event/choice[2]/event/text"
msgstr ""
"\"Vraiment pas ? Ah... Bon, sympa la conversation ! Mais maintenant je dois "
"vous tuer.\""

#: src-en/data/events_sector_duskbringer.xml:433
msgid ""
"data/events_sector_duskbringer.xml$//event[@name=\"DUSK_IDOL\"]/choice/event/choice[2]/event/choice/text"
