"data/events_sector_duskbringer.xml$//event[@name=\"NEBULA_DUSK_INFILTRATION\"]/choice/event/choice/event/choice[2]/text"
msgstr "Non lavoriamo con la Coalizione."

#: src-en/data/events_sector_duskbringer.xml:784
msgid ""
"data/events_sector_duskbringer.xml$//event[@name=\"NEBULA_DUSK_INFILTRATION\"]/choice/event/choice/event/choice[2]/event/text"
msgstr ""
