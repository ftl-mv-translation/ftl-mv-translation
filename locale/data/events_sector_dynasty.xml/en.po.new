#: src-en/data/events_sector_dynasty.xml:140
msgid ""
"data/events_sector_dynasty.xml$//event[@name=\"DYNASTY_CACHE\"]/choice[@req=\"BLUELIST_CREW_NODOORS\"]/event/choice[4]/text"
msgstr "Ignore the cache."

#: src-en/data/events_sector_dynasty.xml:142
msgid ""
"data/events_sector_dynasty.xml$//event[@name=\"DYNASTY_CACHE\"]/choice[@req=\"BLUELIST_CREW_NODOORS\"]/event/choice[4]/event/text"
msgstr "Better not to risk touching Dynasty property in Dynasty space."

