#: src-en/data/events_sector_dynasty.xml:504
msgid ""
"data/events_sector_dynasty.xml$//event[@name=\"NEBULA_DYNASTY_TRINKETS\"]/choice[1]/event/choice[4]/text"
msgstr "No thanks."

#: src-en/data/events_sector_dynasty.xml:510
msgid ""
"data/events_sector_dynasty.xml$//event[@name=\"NEBULA_DYNASTY_TRINKETS\"]/choice[2]/text"
msgstr "Avoid the Ghost."

#: src-en/data/events_sector_dynasty.xml:512
msgid ""
"data/events_sector_dynasty.xml$//event[@name=\"NEBULA_DYNASTY_TRINKETS\"]/choice[2]/event/text"
msgstr "You have better things to do than looking at 'wonders' right now."

#: src-en/data/events_sector_dynasty.xml:514
msgid ""
