#: src-en/data/events_sector_dynasty.xml:1759
msgid ""
"data/events_sector_dynasty.xml$//event[@name=\"NEBULA_DYNASTY_OUTPOST\"]/choice[1]/text"
msgstr "Atacar el puesto avanzado."

#: src-en/data/events_sector_dynasty.xml:1761
