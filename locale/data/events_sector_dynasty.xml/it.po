msgid ""
"data/events_sector_dynasty.xml$//event[@name=\"DYNASTY_SLUGOCC\"]/choice[1]/event/choice/event/text"
msgstr "Alimenti le armi e attacchi una delle navi slugman nella colonia."

#: src-en/data/events_sector_dynasty.xml:162
msgid ""
