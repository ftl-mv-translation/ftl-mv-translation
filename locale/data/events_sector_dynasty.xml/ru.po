msgid ""
"data/events_sector_dynasty.xml$//event[@name=\"DYNASTY_OUTPOST\"]/choice[1]/text"
msgstr "Захватить аванпост."

#: src-en/data/events_sector_dynasty.xml:1716
msgid ""
"data/events_sector_dynasty.xml$//event[@name=\"DYNASTY_OUTPOST\"]/choice[1]/event/text"
msgstr "Если Династия следит за этим сектором, есть шанс, что они уже собрали тонну данных... а данные в любой форме ценны для вас!"

