msgid ""
"data/events_sector_empire.xml$//eventList[@name=\"CRYSTAL_CACHE_MV_BREAK\"]/event[1]/text"
msgstr "Du feuerst alles was du hast, jedoch ist die Kristallbarriere zu stark. Nach einer Zeit gibst du auf und fliegst weiter."

#: src-en/data/events_sector_empire.xml:213
msgid ""
"data/events_sector_empire.xml$//eventList[@name=\"CRYSTAL_CACHE_MV_BREAK\"]/event[2]/text"
