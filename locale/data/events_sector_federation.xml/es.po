#: src-en/data/events_sector_federation.xml:254
msgid "data/events_sector_federation.xml$//event[@name=\"FEDERATION_AMMO\"]/text"
msgstr "Una estación de la Federación está enviando una solicitud de munición a cambio de chatarra. Su alta demanda probablemente se deba al creciente número de bloqueos Rebeldes. Podrían estar dispuestos a ofrecer algunos planos si eres generoso."

#: src-en/data/events_sector_federation.xml:256
msgid ""
"data/events_sector_federation.xml$//event[@name=\"FEDERATION_AMMO\"]/choice[1]/text"
msgstr "Ofrecer 5 misiles por toda la chatarra que tengan."

#: src-en/data/events_sector_federation.xml:266
msgid ""
"data/events_sector_federation.xml$//event[@name=\"FEDERATION_AMMO\"]/choice[2]/text"
msgstr "Ofrecer 10 misiles por reparaciones y algo de combustible extra."

#: src-en/data/events_sector_federation.xml:277
msgid ""
"data/events_sector_federation.xml$//event[@name=\"FEDERATION_AMMO\"]/choice[3]/text"
msgstr "Ofrecer 10 misiles por un aumento."

#: src-en/data/events_sector_federation.xml:287
msgid ""
"data/events_sector_federation.xml$//event[@name=\"FEDERATION_AMMO\"]/choice[4]/text"
msgstr "Ofrecer 15 misiles por un arma."

#: src-en/data/events_sector_federation.xml:296
msgid ""
"data/events_sector_federation.xml$//event[@name=\"FEDERATION_AMMO\"]/choice[5]/text"
msgstr "Ofrecer 20 misiles por pura bondad."

#: src-en/data/events_sector_federation.xml:299
msgid ""
"data/events_sector_federation.xml$//event[@name=\"FEDERATION_AMMO\"]/choice[5]/event/text"
msgstr "“Vaya, ¿hablas en serio? ¡Gracias, tu amabilidad no será olvidada! Me aseguraré de que la flota de la Federación entienda lo que has hecho por ellos.”"

#: src-en/data/events_sector_federation.xml:305
msgid ""
"data/events_sector_federation.xml$//event[@name=\"FEDERATION_AMMO\"]/choice[5]/event/choice/text"
msgstr "Continuar..."

#: src-en/data/events_sector_federation.xml:311
msgid ""
"data/events_sector_federation.xml$//event[@name=\"FEDERATION_AMMO\"]/choice[6]/text"
msgstr "No tenemos nada para vender."

#: src-en/data/events_sector_federation.xml:313
msgid ""
"data/events_sector_federation.xml$//event[@name=\"FEDERATION_AMMO\"]/choice[6]/event/text"
msgstr "Ignoras la estación."

#: src-en/data/events_sector_federation.xml:315
msgid ""
"data/events_sector_federation.xml$//event[@name=\"FEDERATION_AMMO\"]/choice[6]/event/choice[1]/text"
