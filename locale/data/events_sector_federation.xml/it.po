msgid ""
"data/events_sector_federation.xml$//event[@name=\"DISTRESS_FEDERATION_KNIGHTS_AND_KNAVES\"]/choice[1]/event/choice[@req=\"unique_haynes\"]/event/choice/event/choice/text"
msgstr "Continua..."

