#: src-en/data/events_sector_freemantis.xml:509
msgid ""
"data/events_sector_freemantis.xml$//event[@name=\"FREEMANTIS_WEIRD_TRADE\"]/choice[2]/event/text"
msgstr ""
"The Mantis shakes his head, \"Where your sense of fight? Disappointing.\" He"
" ends transmissions."

#: src-en/data/events_sector_freemantis.xml:511
msgid ""
"data/events_sector_freemantis.xml$//event[@name=\"FREEMANTIS_WEIRD_TRADE\"]/choice[2]/event/choice[1]/text"
