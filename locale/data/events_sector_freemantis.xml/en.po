msgid ""
"data/events_sector_freemantis.xml$//event[@name=\"FREEMANTIS_BURIAL\"]/choice[3]/text"
msgstr "Leave them alone."

#: src-en/data/events_sector_freemantis.xml:769
msgid ""
"data/events_sector_freemantis.xml$//event[@name=\"FREEMANTIS_BURIAL\"]/choice[3]/event/text"
msgstr ""
"You aren't willing to harass the Free Mantis and their past. You apologize "
"and leave."

#: src-en/data/events_sector_freemantis.xml:771
msgid ""
"data/events_sector_freemantis.xml$//event[@name=\"FREEMANTIS_BURIAL\"]/choice[3]/event/choice[1]/text"
msgstr "Do something onboard the ship."

#: src-en/data/events_sector_freemantis.xml:775
msgid ""
