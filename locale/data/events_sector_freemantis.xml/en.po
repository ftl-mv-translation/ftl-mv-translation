#: src-en/data/events_sector_freemantis.xml:994
msgid ""
"data/events_sector_freemantis.xml$//event[@name=\"FREEMANTIS_DIPLOMACY\"]/choice[4]/text"
msgstr "Stay out of this."

#: src-en/data/events_sector_freemantis.xml:996
msgid ""
"data/events_sector_freemantis.xml$//event[@name=\"FREEMANTIS_DIPLOMACY\"]/choice[4]/event/text"
msgstr ""
"It's probably wise to not take part in this matter. You fly to a spot with "
"less company."

#: src-en/data/events_sector_freemantis.xml:998
msgid ""
"data/events_sector_freemantis.xml$//event[@name=\"FREEMANTIS_DIPLOMACY\"]/choice[4]/event/choice[1]/text"
