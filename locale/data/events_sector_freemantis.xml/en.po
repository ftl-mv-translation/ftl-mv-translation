#: src-en/data/events_sector_freemantis.xml:750
msgid ""
"data/events_sector_freemantis.xml$//event[@name=\"FREEMANTIS_BURIAL\"]/choice[2]/event/choice[3]/event/text"
msgstr ""
"You hand over some scrap to the Free Mantis, in hopes they'll accept the "
"bribe..."

#: src-en/data/events_sector_freemantis.xml:755
msgid ""
"data/events_sector_freemantis.xml$//event[@name=\"FREEMANTIS_BURIAL\"]/choice[2]/event/choice[3]/event/choice/text"
msgstr "Continue..."

#: src-en/data/events_sector_freemantis.xml:761
