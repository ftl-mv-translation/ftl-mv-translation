#: src-en/data/events_sector_freemantis.xml:1121
msgid ""
"data/events_sector_freemantis.xml$//event[@name=\"DISTRESS_FREEMANTIS_STANDOFF\"]/text"
msgstr "Votre ordinateur de bord semble détecter deux signaux de détresse ici, l'un d'un vaisseau de la Fédération et l'autre d'un vaisseau Mantis Libre."

#: src-en/data/events_sector_freemantis.xml:1123
msgid ""
"data/events_sector_freemantis.xml$//event[@name=\"DISTRESS_FREEMANTIS_STANDOFF\"]/choice/text"
