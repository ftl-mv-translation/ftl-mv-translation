msgid ""
"data/events_sector_gathering.xml$//event[@name=\"ORCHID_INSULT\"]/choice[2]/event/text"
msgstr "Ignoras a los Slugs, sabiendo que probablemente se trata de una trampa."

#: src-en/data/events_sector_gathering.xml:1144
msgid ""
"data/events_sector_gathering.xml$//event[@name=\"ORCHID_INSULT\"]/choice[2]/event/choice[1]/text"
msgstr "Hacer algo abordo de la nave."

