#: src-en/data/events_sector_hacker.xml:452
msgid ""
"data/events_sector_hacker.xml$//event[@name=\"HACKED_HOSTAGES\"]/choice[2]/event/choice/text"
msgstr "Связаться с колонией."

#: src-en/data/events_sector_hacker.xml:458
msgid ""
"data/events_sector_hacker.xml$//event[@name=\"HACKED_HOSTAGES\"]/choice[3]/text"
msgstr "Не обращать внимания на колонию."

#: src-en/data/events_sector_hacker.xml:460
msgid ""
"data/events_sector_hacker.xml$//event[@name=\"HACKED_HOSTAGES\"]/choice[3]/event/text"
