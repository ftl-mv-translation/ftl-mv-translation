#: src-en/data/events_sector_harmony.xml:2051
msgid ""
"data/events_sector_harmony.xml$//event[@name=\"QUEST_ENGI_SLAVE\"]/choice[2]/text"
msgstr "We aren't looking to take any jobs."

#: src-en/data/events_sector_harmony.xml:2053
msgid ""
"data/events_sector_harmony.xml$//event[@name=\"QUEST_ENGI_SLAVE\"]/choice[2]/event/text"
