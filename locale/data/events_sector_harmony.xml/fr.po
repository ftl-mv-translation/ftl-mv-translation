msgid ""
"data/events_sector_harmony.xml$//event[@name=\"ENGI_SALVAGER\"]/choice[1]/event/choice[3]/text"
msgstr "Les combattre."

#: src-en/data/events_sector_harmony.xml:125
msgid ""
"data/events_sector_harmony.xml$//event[@name=\"ENGI_SALVAGER\"]/choice[1]/event/choice[3]/event/text"
msgstr ""
"Communiquer plus loin est inutile. La seule manière d'obtenir un résultat "
"est à travers le combat."

#: src-en/data/events_sector_harmony.xml:128
msgid ""
"data/events_sector_harmony.xml$//event[@name=\"ENGI_SALVAGER\"]/choice[1]/event/choice[3]/event/choice/text"
msgstr "Continuer..."

#: src-en/data/events_sector_harmony.xml:136
msgid ""
"data/events_sector_harmony.xml$//event[@name=\"ENGI_SALVAGER\"]/choice[2]/text"
msgstr "Les ignorer."

#: src-en/data/events_sector_harmony.xml:138
msgid ""
"data/events_sector_harmony.xml$//event[@name=\"ENGI_SALVAGER\"]/choice[2]/event/text"
msgstr ""
"Le vaisseau Lanius continue de vous ignorer tandis que vous vous éloignez."

#: src-en/data/events_sector_harmony.xml:140
msgid ""
"data/events_sector_harmony.xml$//event[@name=\"ENGI_SALVAGER\"]/choice[2]/event/choice[1]/text"
