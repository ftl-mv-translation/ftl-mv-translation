msgid ""
"data/events_sector_hektar.xml$//event[@name=\"NEBULA_DISTRESS_HEKTAR_FEMALE\"]/choice/event/choice[3]/event/text"
msgstr "\"Quelle poissssse, je suis tombé sur le SSSSEUL Renégat qui sssse préoccupe de l'essssclavage. Bon, tant pis, je m'occuperai de ccccette révolte plus tard.\""

#: src-en/data/events_sector_hektar.xml:1104
msgid ""
"data/events_sector_hektar.xml$//event[@name=\"NEBULA_DISTRESS_HEKTAR_FEMALE\"]/choice/event/choice[3]/event/choice/text"
