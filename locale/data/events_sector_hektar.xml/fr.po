msgid ""
"data/events_sector_hektar.xml$//event[@name=\"NEBULA_DISTRESS_HEKTAR_DEBTORS\"]/choice[4]/text"
msgstr "Désolé, nous ne pouvons pas aider."

#: src-en/data/events_sector_hektar.xml:1280
