"data/events_sector_hektar.xml$//event[@name=\"NEBULA_DISTRESS_HEKTAR_CHILDREN\"]/choice[1]/event/choice[2]/text"
msgstr "Aider les protestataires et libérer les enfants."

#: src-en/data/events_sector_hektar.xml:1308
msgid ""
"data/events_sector_hektar.xml$//event[@name=\"NEBULA_DISTRESS_HEKTAR_CHILDREN\"]/choice[1]/event/choice[2]/event/text"
msgstr "Vous activez vos armes pour attaquer l'usine. Un vaisseau arrive pour vous arrêter."

#: src-en/data/events_sector_hektar.xml:1311
msgid ""
"data/events_sector_hektar.xml$//event[@name=\"NEBULA_DISTRESS_HEKTAR_CHILDREN\"]/choice[1]/event/choice[2]/event/choice/text"
