#: src-en/data/events_sector_hektar.xml:405
msgid ""
"data/events_sector_hektar.xml$//event[@name=\"HEKTAR_SUZERAIN\"]/choice[1]/event/choice[@req=\"LIST_VARIANT_HEKTAR\"]/event/text"
msgstr "\"Tu ha uno dei discepoli di Billy! Tu vede grande Billy in carne e ossa! Sono davvero onorato! Per favore, lascia che io accompagna te e impara!\""

#: src-en/data/events_sector_hektar.xml:410
msgid ""
"data/events_sector_hektar.xml$//event[@name=\"HEKTAR_SUZERAIN\"]/choice[1]/event/choice[@req=\"LIST_VARIANT_BILLY\"]/text"
msgstr "(Billy) Presentalo a Billy."

#: src-en/data/events_sector_hektar.xml:412
msgid ""
"data/events_sector_hektar.xml$//event[@name=\"HEKTAR_SUZERAIN\"]/choice[1]/event/choice[@req=\"LIST_VARIANT_BILLY\"]/event/text"
msgstr "\"AAAH! Siete voi! O corrottissimo, Billy in persona! Io non sa cosa dire, troppo emozionato! Per favore, lascia che io accompagna te, sarebbe onore più grande!\""

#: src-en/data/events_sector_hektar.xml:418
