#: src-en/data/events_sector_hive.xml:2415
msgid ""
"data/events_sector_hive.xml$//textList[@name=\"TEXT_ENCOUNTER_MANTIS\"]/text[1]"
msgstr "Neben den Überresten eines Menschenfrachters taucht ein Kriegsschiff der Mantis auf. Mach dich auf einen Kampf gefasst!"

#: src-en/data/events_sector_hive.xml:2416
msgid ""
"data/events_sector_hive.xml$//textList[@name=\"TEXT_ENCOUNTER_MANTIS\"]/text[2]"
msgstr ""

