msgid ""
"data/events_sector_hive.xml$//event[@name=\"QUEST_MANTIS_REFINERY_FREE\"]/choice[3]/text"
msgstr "Drop them all off."

#: src-en/data/events_sector_hive.xml:2105
msgid ""
"data/events_sector_hive.xml$//event[@name=\"QUEST_HIVE_FREEMANTIS_HUNTER\"]/text"
msgstr ""
