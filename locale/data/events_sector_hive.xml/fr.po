#: src-en/data/events_sector_hive.xml:1094
msgid ""
"data/events_sector_hive.xml$//event[@name=\"MANTIS_CREW\"]/choice[@req=\"LIST_CHECK_ANY_CREW\""
" and @lvl=\"1\"]/text"
msgstr "Ouvrir la capsule."

#: src-en/data/events_sector_hive.xml:1098
msgid ""
"data/events_sector_hive.xml$//event[@name=\"MANTIS_CREW\"]/choice[@req=\"LIST_CHECK_ANY_CREW\""
" and @lvl=\"0\"]/text"
msgstr "Il est hors de question que vous risquiez d'ouvrir cette capsule sans personne autour."

#: src-en/data/events_sector_hive.xml:1102
