msgid ""
"data/events_sector_hive.xml$//textList[@name=\"TEXT_ENCOUNTER_MANTIS\"]/text[1]"
msgstr "Una nave militare mantis appare sul radar a corto raggio insieme ai resti di un mercantile umano. Preparati a un incontro ostile!"

#: src-en/data/events_sector_hive.xml:2416
msgid ""
"data/events_sector_hive.xml$//textList[@name=\"TEXT_ENCOUNTER_MANTIS\"]/text[2]"
msgstr ""

#: src-en/data/events_sector_hive.xml:2417
