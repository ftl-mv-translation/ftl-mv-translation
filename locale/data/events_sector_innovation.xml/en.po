#: src-en/data/events_sector_innovation.xml:1341
msgid ""
"data/events_sector_innovation.xml$//event[@name=\"CURA_REVEAL_CONTINUE\"]/choice/event/choice/event/choice[5]/text"
msgstr "7 mini-drone boarding invasion."

#: src-en/data/events_sector_innovation.xml:1343
msgid ""
"data/events_sector_innovation.xml$//event[@name=\"CURA_REVEAL_CONTINUE\"]/choice/event/choice/event/choice[5]/event/text"
