#: src-en/data/events_sector_lanius.xml:752
msgid ""
"data/events_sector_lanius.xml$//eventList[@name=\"LANIUS_TRADER_LIST2_MV\"]/event[1]/text"
msgstr "After the exchange is complete they leave without a word."

#: src-en/data/events_sector_lanius.xml:759
msgid ""
"data/events_sector_lanius.xml$//eventList[@name=\"LANIUS_TRADER_LIST2_MV\"]/event[2]/text"
