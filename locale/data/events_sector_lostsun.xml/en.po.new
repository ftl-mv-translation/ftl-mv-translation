#: src-en/data/events_sector_lostsun.xml:428
msgid ""
"data/events_sector_lostsun.xml$//event[@name=\"LOSTSUN_CHALLENGERS\"]/choice[5]/event/text"
msgstr "\"Let the battle commence!\""

#: src-en/data/events_sector_lostsun.xml:445
msgid ""
"data/events_sector_lostsun.xml$//event[@name=\"LOSTSUN_CHALLENGERS\"]/choice[5]/event/choice/text"
msgstr "Continue..."

#: src-en/data/events_sector_lostsun.xml:451
msgid ""
"data/events_sector_lostsun.xml$//event[@name=\"LOSTSUN_CHALLENGERS\"]/choice[6]/text"
msgstr "We aren't looking for a fight."

#: src-en/data/events_sector_lostsun.xml:453
msgid ""
"data/events_sector_lostsun.xml$//event[@name=\"LOSTSUN_CHALLENGERS\"]/choice[6]/event/text"
msgstr "\"I did not take you for cowards. Oh well, perhaps it is for the best.\""

#: src-en/data/events_sector_lostsun.xml:455
msgid ""
"data/events_sector_lostsun.xml$//event[@name=\"LOSTSUN_CHALLENGERS\"]/choice[6]/event/choice[1]/text"
msgstr "Do something onboard the ship."

