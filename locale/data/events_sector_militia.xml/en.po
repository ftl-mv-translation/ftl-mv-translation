#: src-en/data/events_sector_militia.xml:767
msgid ""
"data/events_sector_militia.xml$//event[@name=\"DISTRESS_MILITIA_OUTPOST\"]/choice[@req=\"weapons\"]/event/choice[3]/event/choice/text"
msgstr "Continue..."

#: src-en/data/events_sector_militia.xml:775
msgid ""
"data/events_sector_militia.xml$//event[@name=\"DISTRESS_MILITIA_OUTPOST\"]/choice[3]/text"
msgstr "We cannot help."

#: src-en/data/events_sector_militia.xml:777
msgid ""
"data/events_sector_militia.xml$//event[@name=\"DISTRESS_MILITIA_OUTPOST\"]/choice[3]/event/text"
msgstr "You decide not to fight the pirates."

#: src-en/data/events_sector_militia.xml:779
msgid ""
"data/events_sector_militia.xml$//event[@name=\"DISTRESS_MILITIA_OUTPOST\"]/choice[3]/event/choice[1]/text"
