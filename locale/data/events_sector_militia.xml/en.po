msgid ""
"data/events_sector_militia.xml$//event[@name=\"MILITIA_RECRUITMENT\"]/choice[2]/text"
msgstr "Contact the Rebels; try to talk them out of it."

#: src-en/data/events_sector_militia.xml:539
msgid ""
"data/events_sector_militia.xml$//event[@name=\"MILITIA_RECRUITMENT\"]/choice[@req=\"cloaking\"]/text"
msgstr ""
