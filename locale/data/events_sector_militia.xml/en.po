msgid "data/events_sector_militia.xml$//event[@name=\"DETECTIVE_TALK_THOM\"]/text"
msgstr "What do you want to ask Thom?"

#: src-en/data/events_sector_militia.xml:1428
msgid ""
"data/events_sector_militia.xml$//event[@name=\"DETECTIVE_TALK_THOM\"]/choice[1]/text"
