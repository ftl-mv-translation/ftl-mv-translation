#: src-en/data/events_sector_militia.xml:1438
msgid ""
"data/events_sector_militia.xml$//event[@name=\"DETECTIVE_TALK_THOM\"]/choice[2]/text"
msgstr "Ask if Thom has any connections with the Rebels."

#: src-en/data/events_sector_militia.xml:1440
msgid ""
"data/events_sector_militia.xml$//event[@name=\"DETECTIVE_TALK_THOM\"]/choice[2]/event/text"
msgstr ""
"Thom shakes his head. \"N-no sir. I've met plenty of Rebels in my time, but "
"I've never been friends with any. I've always been a Federation citizen, and"
" a loyal one at that.\""

#: src-en/data/events_sector_militia.xml:1442
msgid ""
"data/events_sector_militia.xml$//event[@name=\"DETECTIVE_TALK_THOM\"]/choice[2]/event/choice/text"
