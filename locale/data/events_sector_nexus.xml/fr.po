#: src-en/data/events_sector_nexus.xml:1757
msgid ""
"data/events_sector_nexus.xml$//event[@name=\"SYLVAN_NARRATOR_WARNING\"]/choice[1]/text"
msgstr "Arrêtez de vous battre."

#: src-en/data/events_sector_nexus.xml:1763
