#: src-en/data/events_sector_obelisk.xml:1088
msgid ""
"data/events_sector_obelisk.xml$//event[@name=\"ROYAL_ZOO_PURCHASE\"]/choice[@req=\"\"]/text"
msgstr "I think we'll be leaving now."

#: src-en/data/events_sector_obelisk.xml:1094
msgid ""
"data/events_sector_obelisk.xml$//eventList[@name=\"ROYAL_ZOO_LEAVE\"]/event[1]/text"
msgstr ""
"\"A shame. I cannot let you mortals leave while there still might be a "
"chance for great wonders aboard your vessel. Come back here!\""

#: src-en/data/events_sector_obelisk.xml:1097
msgid ""
"data/events_sector_obelisk.xml$//eventList[@name=\"ROYAL_ZOO_LEAVE\"]/event[1]/choice/text"
