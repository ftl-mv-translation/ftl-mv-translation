msgid ""
"data/events_sector_origins.xml$//event[@name=\"VORTIGON_KEY\"]/choice/event/choice[@req=\"medbay\"]/text"
msgstr "(Upg. Medbay) Hurry to the Medbay and heal him."

#: src-en/data/events_sector_origins.xml:1467
msgid ""
"data/events_sector_origins.xml$//event[@name=\"VORTIGON_KEY\"]/choice/event/choice[@req=\"medbay\"]/event/text"
