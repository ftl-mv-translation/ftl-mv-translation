msgid ""
"data/events_sector_pirate.xml$//event[@name=\"QUEST_SMUGGLER_RAID_VAULT\"]/choice/event/choice[5]/event/choice[5]/event/choice[2]/text"
msgstr "Yellow"

#: src-en/data/events_sector_pirate.xml:2748
msgid ""
"data/events_sector_pirate.xml$//event[@name=\"QUEST_SMUGGLER_RAID_VAULT\"]/choice/event/choice[5]/event/choice[5]/event/choice[3]/text"
msgstr "Blue"

#: src-en/data/events_sector_pirate.xml:2750
msgid ""
"data/events_sector_pirate.xml$//event[@name=\"QUEST_SMUGGLER_RAID_VAULT\"]/choice/event/choice[5]/event/choice[5]/event/choice[3]/event/text"
msgstr ""
"The lights on the vault door light up green. You quickly open the door and "
"grab the crate inside. Attached are some co-ordinates that you quickly "
"program into your star map and fly away before the Zoltan can arrive."

#: src-en/data/events_sector_pirate.xml:2756
msgid ""
"data/events_sector_pirate.xml$//event[@name=\"QUEST_SMUGGLER_RAID_VAULT\"]/choice/event/choice[5]/event/choice[5]/event/choice[4]/text"
msgstr "Green"

#: src-en/data/events_sector_pirate.xml:2763
msgid ""
"data/events_sector_pirate.xml$//event[@name=\"QUEST_SMUGGLER_RAID_VAULT\"]/choice/event/choice[5]/event/choice[5]/event/choice[5]/text"
