#: src-en/data/events_sector_pirate.xml:923
msgid ""
"data/events_sector_pirate.xml$//textList[@name=\"PIRATE_CIVILIAN\"]/text[5]"
msgstr "Raggiungi il prossimo faro e subito ricevi una chiamata da una piccola navetta. \"Aiutateci! I pirati ci attaccano!\"."

#: src-en/data/events_sector_pirate.xml:924
msgid ""
"data/events_sector_pirate.xml$//textList[@name=\"PIRATE_CIVILIAN\"]/text[6]"
