#: src-en/data/events_sector_pirate.xml:3231
msgid ""
"data/events_sector_pirate.xml$//event[@name=\"QUEST_TUCO_START\"]/choice[2]/event/text"
msgstr "Вас не интересует эта станция."

#: src-en/data/events_sector_pirate.xml:3233
msgid ""
"data/events_sector_pirate.xml$//event[@name=\"QUEST_TUCO_START\"]/choice[2]/event/choice[1]/text"
msgstr "Заняться чем-либо на корабле."

