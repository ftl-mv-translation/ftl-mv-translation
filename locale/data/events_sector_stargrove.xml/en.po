#: src-en/data/events_sector_stargrove.xml:2538
msgid ""
"data/events_sector_stargrove.xml$//event[@name=\"STARGROVE_SEPARATIST_STORE\"]/choice[4]/event/text"
msgstr "The Separatists makes no acknowledgement of your existence."

#: src-en/data/events_sector_stargrove.xml:2540
msgid ""
"data/events_sector_stargrove.xml$//event[@name=\"STARGROVE_SEPARATIST_STORE\"]/choice[4]/event/choice[1]/text"
