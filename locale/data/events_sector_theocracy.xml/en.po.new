msgid ""
"data/events_sector_theocracy.xml$//eventList[@name=\"ROCK_TEMPLE_RAID\"]/event[2]/text"
msgstr "Inside the temple you find some supplies and a Crystal weapon."

#: src-en/data/events_sector_theocracy.xml:880
msgid ""
"data/events_sector_theocracy.xml$//event[@name=\"ROCK_ASTEROID_DERELICT_SHIP\"]/text"
msgstr ""
"A ship without life forms within a nearby dense asteroid field is giving off"
