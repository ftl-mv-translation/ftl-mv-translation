msgid ""
"data/events_sector_theocracy.xml$//event[@name=\"ROCK_ITEM_SONG_CONTEST\"]/choice[@req=\"LIST_CREW_FLORAL\"]/text"
msgstr "(Floral) Perform some Ctl'Yae swing"

#: src-en/data/events_sector_theocracy.xml:625
msgid ""
"data/events_sector_theocracy.xml$//event[@name=\"ROCK_ITEM_SONG_CONTEST\"]/choice[@req=\"LIST_CREW_FLORAL\"]/event/text"
msgstr ""
"Your Floral plays a bopping swing rendition of a well known Rock song. The "
"Rock audience seem to be enjoying themselves."

#: src-en/data/events_sector_theocracy.xml:627
msgid ""
"data/events_sector_theocracy.xml$//event[@name=\"ROCK_ITEM_SONG_CONTEST\"]/choice[@req=\"LIST_CREW_FLORAL\"]/event/choice/text"
