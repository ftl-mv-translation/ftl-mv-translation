#: src-en/data/events_sector_theocracy.xml:131
msgid ""
"data/events_sector_theocracy.xml$//eventList[@name=\"ROCK_APOLOGY_INTERROGATE\"]/event[1]/text"
msgstr "\"J'ai PÉCHÉ ! J'ai... utilisé les toilettes... et je n'avais plus de papier hygiénique... et j'avais une écriture sainte à proximité et... OH MON DIEU ! Je suis un MONSTRE ! Comment avez-vous pu me faire revivre mes crimes ? Vous êtes cruel ! Trop cruel pour ce monde ! Je vous tuerai d'abord, puis je me tuerai moi-même !\""

#: src-en/data/events_sector_theocracy.xml:134
msgid ""
"data/events_sector_theocracy.xml$//eventList[@name=\"ROCK_APOLOGY_INTERROGATE\"]/event[1]/choice/text"
msgstr "Continuer..."

#: src-en/data/events_sector_theocracy.xml:139
msgid ""
"data/events_sector_theocracy.xml$//eventList[@name=\"ROCK_APOLOGY_INTERROGATE\"]/event[2]/text"
msgstr "\"J'ai PÉCHÉ ! J'ai... utilisé les toilettes... et je n'avais plus de papier hygiénique... et j'avais une écriture sainte à proximité et... OH MON DIEU ! Je suis un MONSTRE ! Oh s'il vous plaît, permettez-moi de me racheter ! De quelque manière que ce soit, peut-être y a-il des faveurs que je puisse rendre pour retrouver le chemin du paradis !\""

#: src-en/data/events_sector_theocracy.xml:145
msgid ""
"data/events_sector_theocracy.xml$//eventList[@name=\"ROCK_APOLOGY_ATTACK\"]/event[1]/text"
msgstr "\"J'ai changé d'avis ! Je ne veux plus mourir ! Je me rachèterai en vous tuant à la place !\""

#: src-en/data/events_sector_theocracy.xml:148
msgid ""
"data/events_sector_theocracy.xml$//eventList[@name=\"ROCK_APOLOGY_ATTACK\"]/event[1]/choice/text"
