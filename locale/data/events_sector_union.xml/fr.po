#: src-en/data/events_sector_union.xml:1253
msgid ""
"data/events_sector_union.xml$//event[@name=\"UNION_FACTORY_QUEST\"]/choice/event/choice[@req=\"rep_comb_union\""
" and @lvl=\"1\"]/event/text"
msgstr "Ils ne semblent pas disposés à aider. Après tout, votre réputation pourrait être meilleure."

#: src-en/data/events_sector_union.xml:1256
msgid ""
"data/events_sector_union.xml$//event[@name=\"UNION_FACTORY_QUEST\"]/choice/event/choice[@req=\"rep_comb_union\""
" and @lvl=\"1\"]/event/choice/text"
