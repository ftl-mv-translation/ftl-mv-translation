msgstr ""
"\"A challenge! So let it be!\" The trash king ushers for one of his vessels "
"to fight you."

#: src-en/data/events_sector_vagabond.xml:208
msgid ""
"data/events_sector_vagabond.xml$//event[@name=\"VAGABOND_TRASHKING\"]/choice[1]/event/choice[1]/event/choice/event/choice[1]/event/choice[2]/event/choice/text"
msgstr "Continue..."

#: src-en/data/events_sector_vagabond.xml:214
msgid ""
"data/events_sector_vagabond.xml$//event[@name=\"VAGABOND_TRASHKING\"]/choice[1]/event/choice[1]/event/choice/event/choice[1]/event/choice[3]/text"
msgstr "No thanks."

#: src-en/data/events_sector_vagabond.xml:220
msgid ""
"data/events_sector_vagabond.xml$//event[@name=\"VAGABOND_TRASHKING\"]/choice[1]/event/choice[1]/event/choice/event/choice[2]/text"
msgstr "Refuse."

#: src-en/data/events_sector_vagabond.xml:222
msgid ""
"data/events_sector_vagabond.xml$//event[@name=\"VAGABOND_TRASHKING\"]/choice[1]/event/choice[1]/event/choice/event/choice[2]/event/text"
msgstr "The trash king growls. \"I said BOW!\""

#: src-en/data/events_sector_vagabond.xml:224
msgid ""
"data/events_sector_vagabond.xml$//event[@name=\"VAGABOND_TRASHKING\"]/choice[1]/event/choice[1]/event/choice/event/choice[2]/event/choice/text"
