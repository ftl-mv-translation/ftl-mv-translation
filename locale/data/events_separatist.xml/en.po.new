#: src-en/data/events_separatist.xml:297
msgid ""
"data/events_separatist.xml$//event[@name=\"SEPERATIST_BODY_SCAN\"]/choice[@req=\"phantom_wraith_alpha\"]/text"
msgstr "(Alpha Wraith) Entice him with your Wraith Alpha."

#: src-en/data/events_separatist.xml:301
msgid ""
"data/events_separatist.xml$//event[@name=\"SEPERATIST_BODY_SCAN\"]/choice[@req=\"shell_radiant\"]/text"
msgstr "(Radiant) Entice him with your Radiant."

#: src-en/data/events_separatist.xml:305
msgid ""
"data/events_separatist.xml$//event[@name=\"SEPERATIST_BODY_SCAN\"]/choice[@req=\"zoltan_martyr\"]/text"
msgstr "(Martyr) Entice him with your Martyr."

#: src-en/data/events_separatist.xml:309
msgid ""
"data/events_separatist.xml$//event[@name=\"SEPERATIST_BODY_SCAN\"]/choice[@req=\"mantis_warlord\"]/text"
msgstr "(Warlord) Entice him with your Warlord."

#: src-en/data/events_separatist.xml:313
msgid ""
"data/events_separatist.xml$//event[@name=\"SEPERATIST_BODY_SCAN\"]/choice[@req=\"slug_ranger\"]/text"
msgstr "(Ranger) Entice him with your Ranger."

#: src-en/data/events_separatist.xml:317
msgid ""
"data/events_separatist.xml$//event[@name=\"SEPERATIST_BODY_SCAN\"]/choice[@req=\"leech_ampere\"]/text"
msgstr "(Ampere) Entice him with your Ampere."

#: src-en/data/events_separatist.xml:321
msgid ""
"data/events_separatist.xml$//event[@name=\"SEPERATIST_BODY_SCAN\"]/choice[@req=\"human_technician\"]/text"
msgstr "(Technician) Entice him with your Technician."

#: src-en/data/events_separatist.xml:325
msgid ""
"data/events_separatist.xml$//event[@name=\"SEPERATIST_BODY_SCAN\"]/choice[@req=\"siren_harpy\"]/text"
msgstr "(Harpy) Entice him with your Harpy."

#: src-en/data/events_separatist.xml:327
msgid ""
"data/events_separatist.xml$//event[@name=\"SEPERATIST_BODY_SCAN\"]/choice[@req=\"siren_harpy\"]/event/text"
msgstr ""
"The Separatist begins to scan the Harpy, but pauses mid-way through. \"WHAT "
"IS THIS?! WHAT [IS] THIS?! WHY DOES THE EYE TASTE OF AGONY?! WHAT ARE "
"-THESE- COLORS?! MY MIND, MY [MIND]! AAAAAAAH!\" The Separatist dissolves "
"almost immediately, leaving nothing behind."

#: src-en/data/events_separatist.xml:331
msgid ""
"data/events_separatist.xml$//event[@name=\"SEPERATIST_BODY_SCAN\"]/choice[@req=\"LIST_CREW_ENGI\"]/text"
msgstr "(Engi) Encourage him to scan the Engi."

#: src-en/data/events_separatist.xml:333
msgid ""
"data/events_separatist.xml$//event[@name=\"SEPERATIST_BODY_SCAN\"]/choice[@req=\"LIST_CREW_ENGI\"]/event/text"
msgstr ""
"\"You wish for me... to become identical to my current state? I fail to "
"understand this logic. I will find someone of higher intelligence to scan.\""

#: src-en/data/events_separatist.xml:337
msgid ""
"data/events_separatist.xml$//event[@name=\"SEPERATIST_BODY_SCAN\"]/choice[22]/text"
msgstr "Refuse."

#: src-en/data/events_separatist.xml:339
msgid ""
"data/events_separatist.xml$//event[@name=\"SEPERATIST_BODY_SCAN\"]/choice[22]/event/text"
msgstr "\"You are most [dull]. Farewell.\" He beams off of the ship."

#: src-en/data/events_separatist.xml:341
msgid ""
"data/events_separatist.xml$//event[@name=\"SEPERATIST_BODY_SCAN\"]/choice[22]/event/choice[1]/text"
msgstr "Do something onboard the ship."

#: src-en/data/events_separatist.xml:345
msgid ""
"data/events_separatist.xml$//event[@name=\"SEPERATIST_BODY_SCAN\"]/choice[22]/event/choice[2]/text"
msgstr "Do nothing."

#: src-en/data/events_separatist.xml:353
msgid ""
"data/events_separatist.xml$//eventList[@name=\"SEPERATIST_BODY_SCAN_ALLOW\"]/event[1]/text"
msgstr ""
"\"Yes, you are of peak physical condition. Satisfaction 98%. I can work out "
"the last 2%. Thank you friend. Accept this gift as gratitude.\" He morphs "
"into an exact copy of you and beams off the ship, leaving behind some "
"droneparts."

#: src-en/data/events_separatist.xml:357
msgid ""
"data/events_separatist.xml$//eventList[@name=\"SEPERATIST_BODY_SCAN_ALLOW\"]/event[2]/text"
msgstr ""
"\"Your body is ugly. I do not wish to take this form. Goodbye.\" He beams "
"off of the ship."

#: src-en/data/events_separatist.xml:362
msgid ""
"data/events_separatist.xml$//eventList[@name=\"SEPERATIST_BODY_SCAN_SOLDIER\"]/event[1]/text"
msgstr ""
"\"Yes, this one will do. Satisfaction 98%. I can work out the last 2%. Thank"
" you friend. Accept this gift as gratitude.\" He morphs into an exact copy "
"of your crew and beams off the ship, leaving behind some droneparts."

#: src-en/data/events_separatist.xml:366
msgid ""
"data/events_separatist.xml$//eventList[@name=\"SEPERATIST_BODY_SCAN_SOLDIER\"]/event[2]/text"
msgstr ""
"\"This form is... magnificent. I shall begin the scan, but the details must "
"be perfect. I shall stay around longer, so that I can shape myself in their "
"form to perfection.\""

#: src-en/data/events_separatist.xml:367
msgid ""
"data/events_separatist.xml$//eventList[@name=\"SEPERATIST_BODY_SCAN_SOLDIER\"]/event[2]/crewMember"
msgstr "L-9678"

#: src-en/data/events_separatist.xml:372
msgid ""
"data/events_separatist.xml$//eventList[@name=\"SEPERATIST_BODY_SCAN_SUZERAIN\"]/event[1]/text"
msgstr ""
"\"Yes, this one will do. Satisfaction 98%. I can work out the last 2%. Thank"
" you friend. Accept this gift as gratitude.\" He morphs into an exact copy "
"of your crew and beams off the ship, leaving behind some droneparts."

#: src-en/data/events_separatist.xml:376
msgid ""
"data/events_separatist.xml$//eventList[@name=\"SEPERATIST_BODY_SCAN_SUZERAIN\"]/event[2]/text"
msgstr ""
"\"This form is... magnificent. I shall begin the scan, but the details must "
"be perfect. I shall stay around longer, so that I can shape myself in their "
"form to perfection.\""

