#: src-en/data/events_shells.xml:2169
msgid ""
"data/events_shells.xml$//event[@name=\"NEBULA_MORPH_JUICE\"]/choice[2]/text"
msgstr "If their cargo is valuable, let's take it!"

#: src-en/data/events_shells.xml:2171
msgid ""
"data/events_shells.xml$//event[@name=\"NEBULA_MORPH_JUICE\"]/choice[2]/event/text"
msgstr ""
"You aren't quite sure what they're exactly carrying, but you sure wouldn't "
"mind taking it!"

#: src-en/data/events_shells.xml:2174
msgid ""
"data/events_shells.xml$//event[@name=\"NEBULA_MORPH_JUICE\"]/choice[2]/event/choice/text"
msgstr "Continue..."

#: src-en/data/events_shells.xml:2180
