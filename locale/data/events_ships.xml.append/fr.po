msgid ""
"data/events_ships.xml.append$//ship[@name=\"SHIP_COALITION_STATION_REPRESENTATIVES\"]/destroyed/choice[@req=\"loc_rock_signalled\""
" and @lvl=\"1\"]/text"
msgstr "L'émissaire de la Théocratie vous contacte."

#: src-en/data/events_ships.xml.append:415
msgid ""
"data/events_ships.xml.append$//ship[@name=\"SHIP_COALITION_STATION_REPRESENTATIVES\"]/destroyed/choice[@req=\"loc_rock_signalled\""
" and @lvl=\"1\"]/event/text"
msgstr "\"Une décision impulsive, mais compréhensible. Nous nous portons mieux en leur absence. Nous devrons simplement vivre avec les autres races de la Coalition pour le moment.\" Le Rocailleux vous paie pour votre temps."

#: src-en/data/events_ships.xml.append:420
