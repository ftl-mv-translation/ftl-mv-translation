msgid ""
"data/events_special_curator.xml$//event[@name=\"CURA_DRAWING\"]/choice/event/choice[2]/event/choice[3]/text"
msgstr "Leave it."

#: src-en/data/events_special_curator.xml:547
msgid ""
"data/events_special_curator.xml$//event[@name=\"CURA_DRAWING\"]/choice/event/choice[2]/event/choice[3]/event/text"
msgstr ""
"\"Yeah I figured. You know, maybe getting into the trash selling business "
"was a bad idea? The minute you put a pricetag on this useless junk, people "
"just walk away.\""

#: src-en/data/events_special_curator.xml:549
msgid ""
"data/events_special_curator.xml$//event[@name=\"CURA_DRAWING\"]/choice/event/choice[2]/event/choice[3]/event/choice/text"
msgstr "Continue..."

