msgstr ""
"Your active task from C.U.R.A. is located here.\n"
"\t\t\t[EASY TASK - ARTIFACT NAME: FIRST CRUSH TESTIMONIAL]"

#: src-en/data/events_special_curator.xml:650
msgid ""
"data/events_special_curator.xml$//event[@name=\"CURA_CRUSH\"]/beaconType/visitedTooltip"
msgstr "Explored location. Nothing left of interest."

