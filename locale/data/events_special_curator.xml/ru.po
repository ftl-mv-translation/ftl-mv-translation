msgstr "Продолжить..."

#: src-en/data/events_special_curator.xml:2186
msgid ""
"data/events_special_curator.xml$//event[@name=\"CURA_OUTPOST_SUCCESS\"]/choice[@req=\"LIST_CHECK_ANY_CREW\""
" and @lvl=\"0\"]/text"
msgstr "Мне нужно идти."

#: src-en/data/events_special_curator.xml:2190
msgid ""
