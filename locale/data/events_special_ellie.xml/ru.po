#: src-en/data/events_special_ellie.xml:278
msgid ""
"data/events_special_ellie.xml$//ship[@name=\"SHIP_HUEGHEY_SECURITY\"]/deadCrew/choice[@req=\"LIST_CREW_ELLIE\""
" and @lvl=\"1\"]/text"
msgstr "Найти Хьюги снова."

#: src-en/data/events_special_ellie.xml:282
msgid ""
"data/events_special_ellie.xml$//ship[@name=\"SHIP_HUEGHEY_SECURITY\"]/deadCrew/choice[@req=\"LIST_CREW_ELLIE\""
" and @lvl=\"0\"]/text"
