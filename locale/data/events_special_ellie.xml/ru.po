#: src-en/data/events_special_ellie.xml:270
msgid ""
"data/events_special_ellie.xml$//ship[@name=\"SHIP_HUEGHEY_SECURITY\"]/destroyed/choice[@req=\"LIST_CREW_ELLIE\""
" and @lvl=\"0\"]/text"
msgstr "Элли не справилась. Сейчас нет причин искать Хьюги."

#: src-en/data/events_special_ellie.xml:275
msgid ""
"data/events_special_ellie.xml$//ship[@name=\"SHIP_HUEGHEY_SECURITY\"]/deadCrew/text"
msgstr ""
