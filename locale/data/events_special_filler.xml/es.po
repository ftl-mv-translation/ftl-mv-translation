msgstr "(Escáner vital) Realizar escaneos avanzados en busca de vida."

#: src-en/data/events_special_filler.xml:383
msgid ""
"data/events_special_filler.xml$//event[@name=\"FILLER_SICK\"]/choice[@req=\"sensors\"]/text"
msgstr ""

#: src-en/data/events_special_filler.xml:385
