msgstr "Refuser."

#: src-en/data/events_special_filler.xml:4632
msgid ""
"data/events_special_filler.xml$//eventList[@name=\"FILLER_MERCENARY_WORK_LIST\"]/event[2]/choice[2]/event/text"
msgstr ""
"\"Parfait, merci. Je me demande bien ce que nous allons pouvoir faire d'eux,"
" maintenant...\" Vous vous préparez à effectuer un saut pour quitter le "
"secteur."

#: src-en/data/events_special_filler.xml:4634
msgid ""
"data/events_special_filler.xml$//eventList[@name=\"FILLER_MERCENARY_WORK_LIST\"]/event[2]/choice[2]/event/choice[1]/text"
msgstr "Faire quelque chose à bord du vaisseau."

