msgid ""
"data/events_special_fuel.xml$//event[@name=\"FUEL_ON_DYNASTY\"]/choice[3]/event/text"
msgstr ""
"\"Then he shall strike you down! Watch most carefully, they could be "
"anywhere!\""

#: src-en/data/events_special_fuel.xml:1290
msgid ""
"data/events_special_fuel.xml$//event[@name=\"FUEL_ON_DYNASTY\"]/choice[3]/event/choice[1]/text"
msgstr "Ok..."

#: src-en/data/events_special_fuel.xml:1294
msgid ""
"data/events_special_fuel.xml$//event[@name=\"FUEL_ON_DYNASTY\"]/choice[3]/event/choice[2]/text"
msgstr "These Ghosts are insane. Attack them before they can do the same!"

#: src-en/data/events_special_fuel.xml:1296
msgid ""
"data/events_special_fuel.xml$//event[@name=\"FUEL_ON_DYNASTY\"]/choice[3]/event/choice[2]/event/text"
