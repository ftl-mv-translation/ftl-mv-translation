#: src-en/data/events_special_fuel.xml:1272
msgid "data/events_special_fuel.xml$//event[@name=\"FUEL_ON_DYNASTY\"]/text"
msgstr ""
"From a distance approaches a Dynasty vessel, lured to your distress call. "
"Their radio static roughly translates to \"The demon's heart still beats! "
"Can you hear it!?\""

#: src-en/data/events_special_fuel.xml:1275
msgid ""
"data/events_special_fuel.xml$//event[@name=\"FUEL_ON_DYNASTY\"]/choice[1]/text"
msgstr "Yes."

#: src-en/data/events_special_fuel.xml:1279
msgid ""
"data/events_special_fuel.xml$//event[@name=\"FUEL_ON_DYNASTY\"]/choice[@req=\"LIST_CREW_GHOST\"]/text"
msgstr "(Ghost Crew) \"Yes.\""

#: src-en/data/events_special_fuel.xml:1281
msgid ""
"data/events_special_fuel.xml$//event[@name=\"FUEL_ON_DYNASTY\"]/choice[@req=\"LIST_CREW_GHOST\"]/event/text"
msgstr ""
"\"Good! Then, beware the heart, traveler! It bleeds, but still it beats!\" "
"You assume that's all, but the Ghosts eject a crate of fuel at you, then "
"jump off!"

#: src-en/data/events_special_fuel.xml:1286
msgid ""
"data/events_special_fuel.xml$//event[@name=\"FUEL_ON_DYNASTY\"]/choice[3]/text"
msgstr "No."

