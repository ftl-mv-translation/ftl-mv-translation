#: src-en/data/events_special_fuel.xml:970
msgid ""
"data/events_special_fuel.xml$/FTL/event[@name=\"FUEL_OFF_ROCK_WRECK\"]/choice[2]/event/text"
msgstr "A los rocoides no les gusta que los alienígenas hurguen en sus pertenencias, por muy decrépitas que parezcan. Estando en su territorio, es mejor no correr riesgos."

#: src-en/data/events_special_fuel.xml:972
