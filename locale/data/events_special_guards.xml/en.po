msgid ""
"data/events_special_guards.xml$//event[@name=\"NEBULA_LIGHT_REINFORCEMENTS_DYNASTY\"]/beaconType/unvisitedTooltip"
msgstr "An unvisited location."

#: src-en/data/events_special_guards.xml:6508
msgid ""
"data/events_special_guards.xml$//event[@name=\"NEBULA_LIGHT_REINFORCEMENTS_DYNASTY\"]/beaconType/visitedTooltip"
msgstr "Explored location. Nothing left of interest."

#: src-en/data/events_special_guards.xml:6509
msgid ""
"data/events_special_guards.xml$//event[@name=\"NEBULA_LIGHT_REINFORCEMENTS_DYNASTY\"]/beaconType/undiscoveredTooltip"
msgstr "An unvisited location."

#: src-en/data/events_special_guards.xml:6511
msgid ""
"data/events_special_guards.xml$//event[@name=\"NEBULA_LIGHT_REINFORCEMENTS_DYNASTY\"]/text"
msgstr ""
"\"Demons and Vagabonds whittle my sanity down to its core, and yet now I "
