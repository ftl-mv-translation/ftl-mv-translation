#: src-en/data/events_special_guards.xml:2535
msgid ""
"data/events_special_guards.xml$//textList[@name=\"TEXT_GUARD_ORCHID_CONTACT\"]/text[1]"
msgstr ""
"The Orchid bows, acknowledging your presence, and waits for your next move."

#: src-en/data/events_special_guards.xml:2536
msgid ""
"data/events_special_guards.xml$//textList[@name=\"TEXT_GUARD_ORCHID_CONTACT\"]/text[2]"
msgstr "The Orchid speaks briefly to say hello and falls silent once again."

#: src-en/data/events_special_guards.xml:2541
msgid ""
"data/events_special_guards.xml$//event[@name=\"GUARD_ORCHID_INTERACT\"]/choice[1]/text"
