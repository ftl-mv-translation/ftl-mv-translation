msgid ""
"data/events_special_guards.xml$//event[@name=\"GUARD_CRYSTAL\"]/choice[3]/text"
msgstr "Attack the guard."

#: src-en/data/events_special_guards.xml:2254
msgid ""
"data/events_special_guards.xml$//event[@name=\"GUARD_CRYSTAL\"]/choice[3]/event/choice/text"
