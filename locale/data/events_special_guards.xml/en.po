"data/events_special_guards.xml$//event[@name=\"GUARD_ORCHID_INTERACT\"]/choice[2]/text"
msgstr "Demand supplies."

#: src-en/data/events_special_guards.xml:2549
msgid ""
"data/events_special_guards.xml$//event[@name=\"GUARD_ORCHID_INTERACT\"]/choice[@req=\"weapons\"]/text"
