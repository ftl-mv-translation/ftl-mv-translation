#: src-en/data/events_special_guards.xml:3863
msgid ""
"data/events_special_guards.xml$//eventList[@name=\"GUARD_AUTO_HACK\"]/event[1]/text"
msgstr "Vos temptatives de pirater le drone se sont révélées inefficaces. Le vaisseau continue son attaque."

#: src-en/data/events_special_guards.xml:3866
msgid ""
"data/events_special_guards.xml$//eventList[@name=\"GUARD_AUTO_HACK\"]/event[1]/choice/text"
msgstr "Continuer..."

#: src-en/data/events_special_guards.xml:3874
msgid ""
