msgid ""
"data/events_special_multiverse.xml$//event[@name=\"SYLVAN_LIZARDS\"]/beaconType/unvisitedTooltip"
msgstr "This is the location of a quest from the Traveling Merchant."

#: src-en/data/events_special_multiverse.xml:3167
