#: src-en/data/events_special_multiverse.xml:3487
msgid ""
"data/events_special_multiverse.xml$//event[@name=\"SYLVAN_MORPHUNLOCK\"]/choice[1]/event/choice[4]/text"
msgstr "Avoid the Morph ship."

#: src-en/data/events_special_multiverse.xml:3489
msgid ""
"data/events_special_multiverse.xml$//event[@name=\"SYLVAN_MORPHUNLOCK\"]/choice[1]/event/choice[4]/event/text"
msgstr ""
"You don't feel prepared for this fight. Maybe under different circumstances "
"you could have fought it, but right now you have other things to worry "
"about."

#: src-en/data/events_special_multiverse.xml:3491
msgid ""
"data/events_special_multiverse.xml$//event[@name=\"SYLVAN_MORPHUNLOCK\"]/choice[1]/event/choice[4]/event/choice[1]/text"
msgstr "Do something onboard the ship."

#: src-en/data/events_special_multiverse.xml:3495
msgid ""
"data/events_special_multiverse.xml$//event[@name=\"SYLVAN_MORPHUNLOCK\"]/choice[1]/event/choice[4]/event/choice[2]/text"
msgstr "Do nothing."

#: src-en/data/events_special_multiverse.xml:3503
msgid ""
"data/events_special_multiverse.xml$//event[@name=\"SYLVAN_MORPHUNLOCK\"]/choice[2]/text"
msgstr "We aren't prepared for this."

#: src-en/data/events_special_multiverse.xml:3505
msgid ""
"data/events_special_multiverse.xml$//event[@name=\"SYLVAN_MORPHUNLOCK\"]/choice[2]/event/text"
msgstr ""
"You just aren't ready for this fight. Alas, it must wait for another day."

#: src-en/data/events_special_multiverse.xml:3507
msgid ""
"data/events_special_multiverse.xml$//event[@name=\"SYLVAN_MORPHUNLOCK\"]/choice[2]/event/choice[1]/text"
