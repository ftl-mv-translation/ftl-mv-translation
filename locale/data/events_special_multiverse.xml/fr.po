msgid ""
"data/events_special_multiverse.xml$//event[@name=\"SYLVAN_MORPHUNLOCK\"]/choice[1]/event/choice[1]/event/choice/event/choice/event/text"
msgstr "\"Kur kur tig.\" Les Morphs arrêtent de tourner et retournent travailler à bord de leur vaisseau. On dirait que tout ça n'a servi à rien."

#: src-en/data/events_special_multiverse.xml:3451
msgid ""
"data/events_special_multiverse.xml$//event[@name=\"SYLVAN_MORPHUNLOCK\"]/choice[1]/event/choice[1]/event/choice/event/choice/event/choice[1]/text"
