#: src-en/data/events_special_multiverse.xml:2890
msgid ""
"data/events_special_multiverse.xml$//textList[@name=\"TEXT_SYLVAN_GENERIC\"]/text[11]"
msgstr "\"Ahhh, étranger, je m'attendais à vous voir iccci. Ccc'est un bon jour pour les zzaffaires, vous zzavez une minute ?\""

