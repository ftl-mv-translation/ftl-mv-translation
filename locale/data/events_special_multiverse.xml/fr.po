msgid ""
"data/events_special_multiverse.xml$//event[@name=\"SYLVAN_MORPHUNLOCK\"]/beaconType/unvisitedTooltip"
msgstr "Ceci est l'emplacement d'une quête du Marchand itinérant."

#: src-en/data/events_special_multiverse.xml:3429
