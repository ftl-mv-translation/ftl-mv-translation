#: src-en/data/events_special_socialize.xml:893
msgid ""
"data/events_special_socialize.xml$//textList[@name=\"TEXT_SOCIALIZE_ANURAK_GENERIC_2_REPEAT\"]/text[20]"
msgstr ""
"\"Très indubitablement. Quoi qu'il en soit, c'est une merveilleuse journée "
