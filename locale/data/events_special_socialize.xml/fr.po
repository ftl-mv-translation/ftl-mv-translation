#: src-en/data/events_special_socialize.xml:910
msgid ""
"data/events_special_socialize.xml$//textList[@name=\"TEXT_SOCIALIZE_ANURAK_GENERIC_2_CHOICE\"]/text[15]"
msgstr "Est-ce que vous écoutez au moins ? Pourquoi vous répétez-vous ?"

#: src-en/data/events_special_socialize.xml:913
msgid ""
"data/events_special_socialize.xml$/FTL/event[@name=\"SOCIALIZE_ANURAK_GENERIC_2\"]/text"
msgstr ""
"\"Ah, bonjour Shellskenbry ! Merveilleuse journée que nous avons !\" Anurak "
"vous salue."

#: src-en/data/events_special_socialize.xml:915
msgid ""
"data/events_special_socialize.xml$/FTL/event[@name=\"SOCIALIZE_ANURAK_GENERIC_2\"]/choice[1]/text"
msgstr "Merci."

#: src-en/data/events_special_socialize.xml:929
