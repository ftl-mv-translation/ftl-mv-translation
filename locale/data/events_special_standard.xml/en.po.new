msgstr "Give up."

#: src-en/data/events_special_standard.xml:2672
msgid ""
"data/events_special_standard.xml$//event[@name=\"STOREEVENT_UNION_WAR_ORPHANS\"]/choice[3]/text"
msgstr "Attack them."

#: src-en/data/events_special_standard.xml:2676
msgid ""
"data/events_special_standard.xml$//event[@name=\"STOREEVENT_UNION_WAR_ORPHANS\"]/choice[4]/text"
msgstr "Tune them out."

#: src-en/data/events_special_standard.xml:2678
msgid ""
"data/events_special_standard.xml$//event[@name=\"STOREEVENT_UNION_WAR_ORPHANS\"]/choice[4]/event/text"
msgstr "They keep trying to contact you, but you continue to ignore them."

#: src-en/data/events_special_standard.xml:2680
msgid ""
"data/events_special_standard.xml$//event[@name=\"STOREEVENT_UNION_WAR_ORPHANS\"]/choice[4]/event/choice[1]/text"
