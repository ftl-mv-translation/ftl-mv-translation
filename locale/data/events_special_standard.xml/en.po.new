#: src-en/data/events_special_standard.xml:5595
msgid ""
"data/events_special_standard.xml$//event[@name=\"STOREEVENT_ROCK_CRYSTALS\"]/choice[2]/event/text"
msgstr ""
"Intruding on the business of multiple Rock ships sounds like suicide. Better"
" to not get involved."

#: src-en/data/events_special_standard.xml:5597
msgid ""
"data/events_special_standard.xml$//event[@name=\"STOREEVENT_ROCK_CRYSTALS\"]/choice[2]/event/choice[1]/text"
msgstr "Do something onboard the ship."

#: src-en/data/events_special_standard.xml:5601
msgid ""
"data/events_special_standard.xml$//event[@name=\"STOREEVENT_ROCK_CRYSTALS\"]/choice[2]/event/choice[2]/text"
