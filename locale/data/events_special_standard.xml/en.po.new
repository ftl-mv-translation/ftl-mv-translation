#: src-en/data/events_special_standard.xml:3138
msgid ""
"data/events_special_standard.xml$//event[@name=\"STOREEVENT_ENGI_PESTER\"]/choice[1]/event/choice[1]/event/choice[@req=\"LIST_CREW_HARMONY\"]/event/text"
msgstr ""
