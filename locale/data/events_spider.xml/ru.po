#: src-en/data/events_spider.xml:869
msgid ""
"data/events_spider.xml$//event[@name=\"DISTRESS_INFESTATION_CRASH\"]/choice[2]/event/text"
msgstr "Вы не знаете, что там внизу, но в этом секторе нельзя доверять никому."

#: src-en/data/events_spider.xml:871
msgid ""
"data/events_spider.xml$//event[@name=\"DISTRESS_INFESTATION_CRASH\"]/choice[2]/event/choice[1]/text"
