msgid ""
"data/events_standard.xml$//event[@name=\"STOREEVENT_SHELL_BOWLING\"]/choice[2]/event/text"
msgstr ""
"You flick on your weapons and engage the Shell station. Pffft, who even "
"likes bowling anyways?"

#: src-en/data/events_standard.xml:7337
msgid ""
"data/events_standard.xml$//event[@name=\"STOREEVENT_SHELL_BOWLING\"]/choice[2]/event/choice/text"
msgstr "Continue..."

