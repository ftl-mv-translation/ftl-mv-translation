msgstr "Forget this."

#: src-en/data/events_standard.xml:6272
msgid ""
"data/events_standard.xml$//event[@name=\"STOREEVENT_SLUG_TRAP\"]/choice[2]/event/choice/event/choice[2]/text"
msgstr "Forget this."

#: src-en/data/events_standard.xml:6274
msgid ""
"data/events_standard.xml$//event[@name=\"STOREEVENT_SLUG_TRAP\"]/choice[2]/event/choice/event/choice[2]/event/text"
msgstr "\"Fine... Not everyone appreciates good dealss...\""

#: src-en/data/events_standard.xml:6278
msgid ""
"data/events_standard.xml$//event[@name=\"STOREEVENT_SLUG_TRAP\"]/choice[2]/event/choice/event/choice[@req=\"LIST_CREW_SLUG\"]/text"
msgstr "(Slug Crew) Our Slug senses someone aboard the ship. Investigate it."

#: src-en/data/events_standard.xml:6280
msgid ""
"data/events_standard.xml$//event[@name=\"STOREEVENT_SLUG_TRAP\"]/choice[2]/event/choice/event/choice[@req=\"LIST_CREW_SLUG\"]/event/text"
msgstr ""
"It looks like the merchant was trying to stall you while someone teleported "
