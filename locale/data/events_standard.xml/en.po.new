"and goods."

#: src-en/data/events_standard.xml:7261
msgid ""
"data/events_standard.xml$//event[@name=\"STOREEVENT_PIRATE_SECURITY\"]/choice[1]/event/choice[1]/text"
msgstr "Do something on-board the ship."

