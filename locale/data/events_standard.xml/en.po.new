#: src-en/data/events_standard.xml:6478
msgid ""
"data/events_standard.xml$//event[@name=\"STOREEVENT_SLUG_ASTEROIDS\"]/choice[@req=\"LIST_CREW_SLUG\"]/event/text"
msgstr ""
