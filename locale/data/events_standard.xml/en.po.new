#: src-en/data/events_standard.xml:6336
msgid ""
"data/events_standard.xml$//eventList[@name=\"STOREEVENT_SLUG_TRAP_LIST\"]/event[3]/choice[@req=\"ANTI_VIRUS\"]/event/choice/text"
msgstr "Continue..."

#: src-en/data/events_standard.xml:6343
msgid ""
"data/events_standard.xml$//eventList[@name=\"STOREEVENT_SLUG_TRAP_LIST\"]/event[4]/text"
