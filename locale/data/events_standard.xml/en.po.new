#: src-en/data/events_standard.xml:6288
msgid ""
"data/events_standard.xml$//event[@name=\"STOREEVENT_SLUG_TRAP\"]/choice[2]/event/choice/event/choice[@req=\"BLUELIST_CREW_SNAIL\"]/event/text"
