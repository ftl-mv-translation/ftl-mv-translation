#: src-en/data/events_standard.xml:3114
msgid ""
"data/events_standard.xml$//event[@name=\"STOREEVENT_ORCHID_KERNELS\"]/choice[@req=\"LIST_CREW_ORCHID\"]/event/text"
msgstr ""
