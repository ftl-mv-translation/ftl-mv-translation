msgid ""
"data/events_standard.xml$//event[@name=\"STOREEVENT_ORCHID_FIRE\"]/choice[2]/text"
msgstr "Offer to help with the fire."

#: src-en/data/events_standard.xml:3210
msgid ""
"data/events_standard.xml$//event[@name=\"STOREEVENT_ORCHID_FIRE\"]/choice[2]/event/text"
