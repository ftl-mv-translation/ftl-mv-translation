msgstr "\"Факел\""

#: src-en/data/text_blueprints.xml.append:37
msgid "data/text_blueprints.xml.append$//text[@name=\"weapon_BEAM_HULL_title\"]"
msgstr "Луч \"Жезл\""

#: src-en/data/text_blueprints.xml.append:38
msgid "data/text_blueprints.xml.append$//text[@name=\"weapon_BEAM_HULL_short\"]"
