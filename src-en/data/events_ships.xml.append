														<modifyPursuit amount="-2"/>
													</event>
												</choice>
											</event>
										</choice>
										<choice>
											<text>Take the weapon and leave.</text>
											<event>
												<weapon name="RANDOM"/>
											</event>
										</choice>
									</event>
								</choice>
